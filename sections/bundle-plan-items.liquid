<script>
    function filterBundle() {
        var filter_1 = $('.bundle-plan-list .filter-1').val();
        var filter_2 = $('.bundle-plan-list .filter-2').val();
        
        $('.bundle-plan-list .bottom-title').hide();
        $('.bundle-plan-list .bottom-descr').hide();
        $('.bundle-plan-list .middle-text').show();

        if (filter_1 == 'me-partner') {
            $('.bundle-plan-list .bundle-item-container').hide();
            $('.bundle-plan-list .bundle-item-container[data-filterkey=conception]').show();
        } else if (filter_1 == 'me') {
            if (filter_2 == 'complete-support') {
                $('.bundle-plan-list .bundle-item-container').hide();
                $('.bundle-plan-list .bundle-item-container[data-filterkey=complete]').show();
            } else if (filter_2 == 'basic') {
                $('.bundle-plan-list .bundle-item-container').hide();
                $('.bundle-plan-list .bundle-item-container[data-filterkey=starter]').show();
            }
        } else if (filter_1 == 'partner') {
            $('.bundle-plan-list .bundle-item-container').hide();
            $('.bundle-plan-list .bundle-item-container[data-filterkey=mens-support]').show();
            $('.bundle-plan-list .bottom-title').show();
            $('.bundle-plan-list .bottom-descr').show();
            $('.bundle-plan-list .middle-text').hide();
        }
    }

    $('.bundle-plan-list .filter-list select').on('change', function() {
        filterBundle();
    });

    $(document).ready(function() {
        $('.bundle-plan-list').on('click', '.bundle-reviews .stamped-product-reviews-badge', function() {
            var idx = $(this).data('idx');
            var title = $(this).data('product-title');
            var product_list = '{{ product_list }}';

            product_list = product_list.split('&');
            var review_html = '<div id="stamped-main-widget" class="stamped-main-widget" data-widget-style="standard" data-product-id="' + product_list[idx] + '"></div>';
            $('.bundle-plan-reviews-sidebar .sidebar-body').html(review_html);
            $('.bundle-plan-reviews-sidebar .sidebar-header .bundle-plan-title').html(title);
            StampedFn.reloadUGC();
            $('.bundle-plan-reviews-sidebar').addClass('active');
            $('.bundle-plan-reviews-sidebar-overlay').addClass('active');
            $('body').css('overflow', 'hidden');
        });
        $('.bundle-plan-reviews-sidebar-overlay').on('click', function() {
            if ($(this).hasClass('active')) {
                $('body').css('overflow', 'unset');
                $('.bundle-plan-reviews-sidebar').removeClass('active');
                $(this).removeClass('active');
            }
        });
        $('.bundle-plan-reviews-sidebar').on('click', '.sidebar-close', function() {
            if ($('.bundle-plan-reviews-sidebar').hasClass('active')) {
                $('body').css('overflow', 'unset');
                $('.bundle-plan-reviews-sidebar').removeClass('active');
                $('.bundle-plan-reviews-sidebar-overlay').removeClass('active');
            }
        }); 

        $('#shopify-section-bundle-plan-items .btn-bundle-plan').click(function() {
            $(this).parents('.bundle-item-container').find('.bundle-popup').addClass('active');
            $('.bundle-popup-overlay').addClass('active');
            $('body').css('overflow', 'hidden');
			$(this).parents('.bundle-item-container').find('.bundle-popup form.active').find("#rc_container").show();
            setTimeout(function(){
				$(this).parents('.bundle-item-container').find('.bundle-popup form.active').find(".rc_radio.rc_radio__autodeliver").click();
				$(this).parents('.bundle-item-container').find('.bundle-popup form.active').find(".rc_radio.rc_radio__autodeliver").trigger();
			}, 300);
        });
        $('.bundle-popup-overlay').on('click', function() {
            if ($(this).hasClass('active')) {
                $('body').css('overflow', 'unset');
                $('.bundle-popup.active').find('.pc-radio.first').click();
                $('#shopify-section-bundle-plan-items .bundle-popup.active').removeClass('active');
                $(this).removeClass('active');
                $('.bundle-popup').find('.powder-element').addClass('active');
                $('.bundle-popup').find('.capsule-element').removeClass('active');
                $('.bundle-popup').find('.essential-element').removeClass('active');
            }
        });
        $('.bundle-popup').on('click', '.bundle-popup-close', function() {
            if ($('.bundle-popup').hasClass('active')) {
                $('body').css('overflow', 'unset');
                $('.bundle-popup').removeClass('active');
                $('.bundle-popup-overlay').removeClass('active');
                $('.bundle-popup').find('.powder-element').addClass('active');
                $('.bundle-popup').find('.capsule-element').removeClass('active');
                $('.bundle-popup').find('.essential-element').removeClass('active');
                $(this).parents('.bundle-popup').find('.pc-radio.first').click();
            }
        });

        $(".bundle-popup .pc-radio").on('change', function() {
            var option = $(this).val();

            if (option == "powder") {
            $('.bundle-popup').find('.powder-element').addClass('active');
            $('.bundle-popup').find('.capsule-element').removeClass('active');
            $('.bundle-popup').find('.essential-element').removeClass('active');
            } else if (option == "capsule") {
            $('.bundle-popup').find('.powder-element').removeClass('active');
            $('.bundle-popup').find('.capsule-element').addClass('active');
            $('.bundle-popup').find('.essential-element').removeClass('active');
            } else if (option == "essential") {
            $('.bundle-popup').find('.powder-element').removeClass('active');
            $('.bundle-popup').find('.capsule-element').removeClass('active');
            $('.bundle-popup').find('.essential-element').addClass('active');
            }
        });
    });
</script>

