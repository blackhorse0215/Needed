                                {% if block.settings.popup_description != blank %}
                                    {{ block.settings.popup_description }}
                                {% else %}
                                    {{ block.settings.bundle_description }}
                                {% endif %}
                            </div>
                    
                            <div class="option-list">
                                <div class="product-option">
                                <input type="radio" class="pc-radio first" name="product_option_{{ block.id }}" id="option_powder-{{ block.id }}" value="powder" checked>
                                <label for="option_powder-{{ block.id }}" class="pc-option-label">Prenatal Multi Powder -&nbsp;<span>{{ block.settings.powder_bundled_subscription | times: 100 | money }}</span></label>
                                </div>
                                {% if block.settings.capsule_show %}
                                <div class="product-option">
                                <input type="radio" class="pc-radio" name="product_option_{{ block.id }}" id="option_capsule-{{ block.id }}" value="capsule">
                                <label for="option_capsule-{{ block.id }}" class="pc-option-label">Prenatal Multi Capsules -&nbsp;<span>{{ block.settings.capsule_bundled_subscription | times: 100 | money }}</span></label>
                                </div>
                                {% endif %}
                                {% if block.settings.essential_show %}
                                <div class="product-option">
                                <input type="radio" class="pc-radio" name="product_option_{{ block.id }}" id="option_essential-{{ block.id }}" value="essential">
                                <label for="option_essential-{{ block.id }}" class="pc-option-label">Prenatal Multi Essentials -&nbsp;<span>{{ block.settings.essential_bundled_subscription | times: 100 | money }}</span></label>
                                {% if block.settings.difference_url != blank %}
                                <span class="option-difference"><a href="{{ block.settings.difference_url }}" class="open-link">What’s the difference</a></span>
                                {% endif %}
                                </div>
                                {% endif %}
                            </div>
                    
                            <div class="product-prices">
                                <div class="subscription-price">
                                <div class="label-subscription">Bundled Subscription - ships monthly</div>
                                <div class="content-subscription powder-element active">
                                    {{ block.settings.powder_bundled_subscription | times: 100 | money }}
                                    <span class="saving-label">{{ block.settings.powder_retail_price | minus: block.settings.powder_bundled_subscription | round | times: 100 | money_without_trailing_zeros }} savings</span>
                                </div>
                                {% if block.settings.capsule_show %}
                                <div class="content-subscription capsule-element">
                                    {{ block.settings.capsule_bundled_subscription | times: 100 | money }}
                                    <span class="saving-label">{{ block.settings.capsule_retail_price | minus: block.settings.capsule_bundled_subscription | round | times: 100 | money_without_trailing_zeros }} savings</span>
                                </div>
                                {% endif %}
                                {% if block.settings.essential_show %}
                                <div class="content-subscription essential-element">
                                    {{ block.settings.essential_bundled_subscription | times: 100 | money }}
                                    <span class="saving-label">{{ block.settings.essential_retail_price | minus: block.settings.essential_bundled_subscription | round | times: 100 | money_without_trailing_zeros }} savings</span>
                                </div>
                                {% endif %}
                                </div>
                                <div class="retail-price">
                                <div class="label-retail">Retail Price</div>
                                <div class="content-retail powder-element active">{{ block.settings.powder_retail_price }}</div>
                                {% if block.settings.capsule_show %}
                                <div class="content-retail capsule-element">{{ block.settings.capsule_retail_price }}</div>
                                {% endif %}
                                {% if block.settings.essential_show %}
                                <div class="content-retail essential-element">{{ block.settings.essential_retail_price }}</div>
                                {% endif %}
                                </div>
                            </div>
                    
                            <div class="additional-info-wrapper">
                                <div class="additional-info powder-element active">{{ block.settings.powder_additional_info }}</div>
                                {% if block.settings.capsule_show %}
                                <div class="additional-info capsule-element">{{ block.settings.capsule_additional_info }}</div>
                                {% endif %}
                                {% if block.settings.essential_show %}
                                <div class="additional-info essential-element">{{ block.settings.essential_additional_info }}</div>
                                {% endif %}
                            </div>
                    
                            <div class="buttons-wrapper">
                                <form action="/cart/add" method="post" enctype="multipart/form-data" class="powder-element active" data-product-original-form="" data-productid="{{ all_products[block.settings.powder_bundle_product].id }}">
                                {% assign product = all_products[block.settings.powder_bundle_product] %}
                                {% assign bundle_collection_fg = 'true' %}
                                {% if product.available %}{% include 'preorder-product' %}{% endif %}
                                <input type="hidden" id="quantity" name="quantity" value="1">
                                <input type="hidden" class="hidden-variant-id" name="id" value="{{ all_products[block.settings.powder_bundle_product].variants[0].id }}">
                                <button type="submit" name="add" class="btn-bundle-plan">{{ block.settings.powder_bundle_button }}</button>
                                </form>
                                {% if block.settings.capsule_show %}
                                <form action="/cart/add" method="post" enctype="multipart/form-data" class="capsule-element" data-product-original-form="" data-productid="{{ all_products[block.settings.capsule_bundle_product].id }}">
                                {% assign product = all_products[block.settings.capsule_bundle_product] %}
                                {% assign bundle_collection_fg = 'true' %}
                                {% if product.available %}{% include 'preorder-product' %}{% endif %}
                                <input type="hidden" id="quantity" name="quantity" value="1">
                                <input type="hidden" class="hidden-variant-id" name="id" value="{{ all_products[block.settings.capsule_bundle_product].variants[0].id }}">
                                <button type="submit" name="add" class="btn-bundle-plan">{{ block.settings.capsule_bundle_button }}</button>
                                </form>
                                {% endif %}
                                {% if block.settings.essential_show %}
                                <form action="/cart/add" method="post" enctype="multipart/form-data" class="essential-element" data-product-original-form="" data-productid="{{ all_products[block.settings.essential_bundle_product].id }}">
                                {% assign product = all_products[block.settings.essential_bundle_product] %}
                                {% assign bundle_collection_fg = 'true' %}
                                {% if product.available %}{% include 'preorder-product' %}{% endif %}
                                <input type="hidden" id="quantity" name="quantity" value="1">
                                <input type="hidden" class="hidden-variant-id" name="id" value="{{ all_products[block.settings.essential_bundle_product].variants[0].id }}">
                                <button type="submit" name="add" class="btn-bundle-plan">{{ block.settings.essential_bundle_button }}</button>
                                </form>
                                {% endif %}
                            </div>
                    
                            <div class="bundle-comments">{{ block.settings.bundle_comments }}</div>
                        </div>
                    </div>
                </div>
                <div class="bundle-popup-close"></div>
            </div>
        </div>
        {%- endcapture -%}

        {% assign bundle_list2 = bundle_list2 | append: bundle_html %}
    {% endif %}

