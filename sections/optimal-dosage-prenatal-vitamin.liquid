<section class="optimal-dosage-pvitamin">
  <div class="container" id="optimal_dosage_pvitamin">
    <div class="row">
      <h2 class="header-title optimal-dosage-pvitamin__title">{{ section.settings.section_title }}</h2>
      {% if section.settings.subtitle != blank %}
      <div class="optimal-dosage-pvitamin__subtitle">
        {{ section.settings.subtitle }}
      </div>
      {% endif %}
    </div>
    
    {% assign no_testing_count = 0 %}
    {% capture block_no_testing %}
      {% if section.blocks.size > 0 %}
        <div class="no_testing_block_wrapper">
        {% for block in section.blocks %}
          {% if block.type == 'no_testing' %}
            {% assign no_testing_count = 1 %}
            <div class="no_testing no_testing-{{ block.id }}">
              <div class="no_testing_title">
                <p>{{ block.settings.title }}</p>
              </div>
              <div class="no_testing_content">
                {% assign capsule_count = block.settings.capsule_count | times: 1 %}
                {% if capsule_count > 0 %}
                <div class="capsule-images">
                  {% for i in (1..capsule_count) %}
                  <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                  {% endfor %}
                </div>
                {% endif %}
                <p>
                  {% if block.settings.capsule_count == '' %}
                  * See below
                  {% elsif capsule_count <= 1 %}
                  {{ block.settings.capsule_count | append: ' capsule / day' }}
                  {% elsif capsule_count > 1 %}
                  {{ block.settings.capsule_count | append: ' capsules / day' }}
                  {% endif %}
                </p>
                <div class="dosage">{{ block.settings.dosage }}</div>
              </div>
              <style>
                .optimal-dosage-pvitamin .no_testing-{{ block.id }} .no_testing_content {
                  background-color: {{ block.settings.bg_color }};
                }
                @media (max-width: 767px) {
                  .optimal-dosage-pvitamin .no_testing-{{ block.id }} .no_testing_content {
                    border: 1px solid #F6F4F2;
                  }
                }
              </style>
            </div>
          {% endif %}
        {% endfor %}
        </div>
      {% endif %}
    {% endcapture %}

    {% if no_testing_count == 1 %}
    <div class="row no_testing-row">
        <div class="blocktitle_wrapper">
          <h3 class="no_testing_blocktitle">{{ section.settings.no_testing_title }}</h3>
        </div>
        <div class="no_testing_blocks_wrapper">
        {{ block_no_testing }}
        </div>
    </div>
    {% endif %}

    {% assign testing_count = 0 %}
    {% capture block_testing %}
      {% if section.blocks.size > 0 %}
        <div class="testing_block_wrapper desktop">
          <div class="testing testing-fixed">
            <div class="testing_title">Life Stage</div>
            <div class="testing_content1">Pregnancy and Fertility Focused or Mom Life</div>
            <div class="testing_content2">Breastfeeding and Postpartum &lt;1 year</div>
          </div>
        {% for block in section.blocks %}
          {% if block.type == 'testing' %}
            {% assign testing_count = 1 %}
            <div class="testing testing-{{ block.id }}">
              <div class="testing_title">
                <p>{{ block.settings.title }}</p>
              </div>
              <div class="testing_content1">
                {% assign capsule_count1 = block.settings.capsule_count1 | times: 1 %}
                {% if capsule_count1 > 0 %}
                <div class="capsule-images">
                  {% for i in (1..capsule_count1) %}
                  <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                  {% endfor %}
                </div>
                {% endif %}
                <p>
                  {% if block.settings.capsule_count1 == '' %}
                  * See below
                  {% elsif capsule_count1 <= 1 %}
                  {{ block.settings.capsule_count1 | append: ' capsule / day' }}
                  {% elsif capsule_count1 > 1 %}
                  {{ block.settings.capsule_count1 | append: ' capsules / day' }}
                  {% endif %}
                </p>
                <div class="dosage">{{ block.settings.dosage1 }}</div>
                {% if block.settings.chk_or == true %}
                <div class="tag-or">OR</div>  
                {% endif %}
              </div>
              <div class="testing_content2">
                {% assign capsule_count2 = block.settings.capsule_count2 | times: 1 %}
                {% if capsule_count2 > 0 %}
                <div class="capsule-images">
                  {% for i in (1..capsule_count2) %}
                  <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                  {% endfor %}
                </div>
                {% endif %}
                <p>
                  {% if block.settings.capsule_count2 == '' %}
                  * See below
                  {% elsif capsule_count2 <= 1 %}
                  {{ block.settings.capsule_count2 | append: ' capsule / day' }}
                  {% elsif capsule_count2 > 1 %}
                  {{ block.settings.capsule_count2 | append: ' capsules / day' }}
                  {% endif %}
                </p>
                <div class="dosage">{{ block.settings.dosage2 }}</div>
                {% if block.settings.footer_text != blank %}
                <div class="footer-text">{{ block.settings.footer_text }}</div>
                {% endif %}
              </div>
              <style>
                .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content1,
                .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                  background-color: {{ block.settings.bg_color }};
                }
                @media (max-width: 767px) {
                  .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content1,
                  .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                    border: 1px solid #F6F4F2;
                  }
                }
              </style>
            </div>
          {% endif %}
        {% endfor %}
        </div>

        <div class="testing_block_wrapper mobile">
          <div class="testing_content-heading">Pregnancy and Fertility Focused or Mom Life</div>
          <div class="testing_content-body">
            {% for block in section.blocks %}
              {% if block.type == 'testing' %}
                {% assign testing_count = 1 %}
                <div class="testing testing-{{ block.id }}{% if block.settings.chk_or == true %} testing-fullwidth{% endif %}">
                  <div class="testing_title">
                    <p>{{ block.settings.title }}</p>
                  </div>
                  
                  {% if block.settings.chk_or == true %}
                  <div class="testing_content-wrapper">
                  {% endif %}
                    <div class="testing_content1">
                      {% assign capsule_count1 = block.settings.capsule_count1 | times: 1 %}
                      {% if capsule_count1 > 0 %}
                      <div class="capsule-images">
                        {% for i in (1..capsule_count1) %}
                        <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                        {% endfor %}
                      </div>
                      {% endif %}
                      <p>
                        {% if block.settings.capsule_count1 == '' %}
                        * See below
                        {% elsif capsule_count1 <= 1 %}
                        {{ block.settings.capsule_count1 | append: ' capsule / day' }}
                        {% elsif capsule_count1 > 1 %}
                        {{ block.settings.capsule_count1 | append: ' capsules / day' }}
                        {% endif %}
                      </p>
                      <div class="dosage">{{ block.settings.dosage1 }}</div>
                      {% if block.settings.chk_or == true %}
                      <div class="tag-or">OR</div>  
                      {% endif %}
                    </div>
                    {% if block.settings.chk_or == true %}
                    <div class="testing_content2">
                      {% assign capsule_count2 = block.settings.capsule_count2 | times: 1 %}
                      {% if capsule_count2 > 0 %}
                      <div class="capsule-images">
                        {% for i in (1..capsule_count2) %}
                        <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                        {% endfor %}
                      </div>
                      {% endif %}
                      <p>
                        {% if block.settings.capsule_count2 == '' %}
                        * See below
                        {% elsif capsule_count2 <= 1 %}
                        {{ block.settings.capsule_count2 | append: ' capsule / day' }}
                        {% elsif capsule_count2 > 1 %}
                        {{ block.settings.capsule_count2 | append: ' capsules / day' }}
                        {% endif %}
                      </p>
                      <div class="dosage">{{ block.settings.dosage2 }}</div>
                      {% if block.settings.footer_text != blank %}
                      <div class="footer-text">{{ block.settings.footer_text }}</div>
                      {% endif %}
                    </div>
                    {% endif %}
                    <style>
                      .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content1,
                      .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                        background-color: {{ block.settings.bg_color }};
                      }
                      @media (max-width: 767px) {
                        .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content1,
                        .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                          border: 1px solid #F6F4F2;
                        }
                      }
                    </style>
                  {% if block.settings.chk_or == true %}
                  </div>
                  {% endif %}
                </div>
              {% endif %}
            {% endfor %}
          </div>

          <div class="testing_content-heading">Breastfeeding and Postpartum &lt;1 year</div>
          <div class="testing_content-body">
          {% for block in section.blocks %}
            {% if block.type == 'testing' %}
              {% assign testing_count = 1 %}
              <div class="testing testing-{{ block.id }}">
                <div class="testing_title">
                  <p>{{ block.settings.title }}</p>
                </div>
                <div class="testing_content2">
                  {% assign capsule_count2 = block.settings.capsule_count2 | times: 1 %}
                  {% if capsule_count2 > 0 %}
                  <div class="capsule-images">
                    {% for i in (1..capsule_count2) %}
                    <img class="lazyload image-capsule" data-src="{{ 'capsule1.png' | asset_img_url: '22x60' }}" alt="">
                    {% endfor %}
                  </div>
                  {% endif %}
                  <p>
                    {% if block.settings.capsule_count2 == '' %}
                    * See below
                    {% elsif capsule_count2 <= 1 %}
                    {{ block.settings.capsule_count2 | append: ' capsule / day' }}
                    {% elsif capsule_count2 > 1 %}
                    {{ block.settings.capsule_count2 | append: ' capsules / day' }}
                    {% endif %}
                  </p>
                  <div class="dosage">{{ block.settings.dosage2 }}</div>
                </div>
                <style>
                  .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                    background-color: {{ block.settings.bg_color }};
                  }
                  @media (max-width: 767px) {
                    .optimal-dosage-pvitamin .testing-{{ block.id }} .testing_content2 {
                      border: 1px solid #F6F4F2;
                    }
                  }
                </style>
              </div>
            {% endif %}
          {% endfor %}
          </div>
        </div>
      {% endif %}
    {% endcapture %}

    {% if testing_count == 1 %}
    <div class="row testing-row">
      <div class="blocktitle_wrapper">
        <h3 class="testing_blocktitle">{{ section.settings.testing_title }}</h3>
        <p class="testing_blocksubtitle">{{ section.settings.testing_subtitle }}</p>
      </div>
      <div class="testing_blocks_wrapper">
      {{ block_testing }}
      </div>
    </div>
    {% endif %}
  </div>
</section>

