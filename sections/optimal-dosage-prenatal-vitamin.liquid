{% schema %}
{
  "name": "Optimal dosage",
  "settings": [
    {
      "type": "text",
      "id": "section_title",
      "label": "Heading",
      "default": "Find your optimal dosage"
    },
    {
      "type": "textarea",
      "id": "subtitle",
      "label": "Sub Heading"
    },
    {
      "type": "textarea",
      "id": "no_testing_title",
      "label": "No Testing Title"
    },
    {
      "type": "textarea",
      "id": "testing_title",
      "label": "Testing Title"
    },
