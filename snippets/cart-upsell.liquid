<script>

    function addItemToCart(variant_id, qty) {

        data = {
            id: variant_id,
            quantity: qty,
            "properties": {
                "shipping_interval_frequency": 30,
                "shipping_interval_unit_type": "Days"
            }
        }

        jQuery.ajax({
            type: 'POST',
            url: '/cart/add.js',
            data: data,
            dataType: 'json',
            success: function() { 
                window.location.href = '/cart'; 
            }
        });
    }

    function changeItems(replaceIds, index, productId){
        if(replaceIds.length == index){
            addItemToCart(productId, 1);
            return;
        }
        
        data = {
            id: replaceIds[index],
            quantity: 0
        }

        $.ajax({
            type: 'POST',
            url: '/cart/change.js',
            data: data,
            dataType: 'json',
            success: function() { 
                changeItems(replaceIds, ++index, productId);
            }
        });
    }

    $(function(){
        $(".btn-subscribe-plan").on("click", function(){
            var productId = $("#upsell_product_id").val();
            var replaceIds = $("#cart_replace_ids").val();
            if (replaceIds != '') {
                replaceIds = replaceIds.split(",");
            }
            
            changeItems(replaceIds, 0, productId);
        })

        $(".btn-subscribe-upsell").on("click", function(){
            var productId = $("#upsell_product_id").val();
            var replaceKey = $("#cart_replace_key").val();
            var productId2 = $("#upsell_product_id2").val();
            var event_category = $('.cart-item:first-child .item-detail--info .product-info .cart-item__title h2').text().trim();
            var event_label = $('.routine-title').text();
            dataLayer.push({
                'event': 'onClick',
                'category' : event_category,
                'action': 'Upsell ATC',
                'label': event_label
            });
            
            if (productId == undefined && replaceKey == undefined) {
                if (productId2 == 39636455260312) {
                    add_data = {
                        id: productId2,
                        quantity: 1,
                        'purchase_type': 'autodeliver',
                        'properties': {
                            "subscription_id": $('.upsell-subscription #rc_subscription_id').val(),
                            "shipping_interval_frequency": 30,
                            "shipping_interval_unit_type": "Days"
                        }
                    }

                    jQuery.ajax({
                        type: 'POST',
                        url: '/cart/add.js',
                        data: add_data,
                        dataType: 'json',
                        success: function() { 
                            window.location.href = '/cart';
                        }
                    });
                } else {
                    add_data = {
                        id: productId2,
                        quantity: 1
                    }
                    jQuery.ajax({
                        type: 'POST',
                        url: '/cart/add.js',
                        data: add_data,
                        dataType: 'json',
                        success: function() { 
                            window.location.href = '/cart';
                        }
                    });
                }
            } else {
                data = {
                    id: replaceKey,
                    quantity: 0
                }

                $.ajax({
                    type: 'POST',
                    url: '/cart/change.js',
                    data: data,
                    dataType: 'json',
                    success: function() { 

                        var data2 = {
                            id: productId,
                            quantity: 1,
                            "properties": {
                                "shipping_interval_frequency": 30,
                                "shipping_interval_unit_type": "Days"
                            }
                        }

                        jQuery.ajax({
                            type: 'POST',
                            url: '/cart/add.js',
                            data: data2,
                            dataType: 'json',
                            success: function() { 

                                addItemToCart(productId2, 1);
                            }
                        });
                    }
                });
            }
        })
    })
    
</script>