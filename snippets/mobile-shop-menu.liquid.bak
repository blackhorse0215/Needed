<script>
    $(function(){
        $(".header__container--mobile .mobile-nav-menu-shop").on("click", function(){
            $(".mobile-shop-menu-container").fadeIn("fast");
            $(".mobile-top-menu-header").fadeIn("fast");
            // $("html, body").animate({ scrollTop: "0" });
            $('#zonos.top.intl').css('display', 'none');
            $('.smile-launcher-frame-container.smile-launcher-mobile').css('display', 'none');
            $('#intercom-container').css('display', 'none');
            $('.intercom-lightweight-app').css('display', 'none');
            $('body').addClass('open-mobile-menu');
        });
        
        $(".mobile-top-menu-header .close-mobile-menu").on("click", function(){
            $(".mobile-shop-menu-container").fadeOut("fast");
            $(".mobile-top-menu-header").fadeOut("fast");
            $('#zonos.top.intl').css('display', 'block');
            $('.smile-launcher-frame-container.smile-launcher-mobile').css('display', 'block');
            $('#intercom-container').css('display', 'block');
            $('.intercom-lightweight-app').css('display', 'block');
            $('body').removeClass('open-mobile-menu');
        });
    })
</script>