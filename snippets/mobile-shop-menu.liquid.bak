
<style>
.mobile-shop-menu-container{
    /* position: absolute; */
    background: #FFF;
    width: 100%;
    padding: 20px 15px 60px;
    z-index: 10000;
    margin-top: 55px;

    display: none;
    position: fixed;
    height: calc(100% - 55px);
    overflow-y: auto;
    top: unset !important;
}

.mobile-shop-menu-container .mobile-plan-menu-label{
    font-family: "BrownPro-Regular";
    font-size: 14px;
    color: #1F3337;
    opacity: 0.5;
}

.mobile-shop-menu-container .mega-menu.mega-menu-plan {
    padding-bottom: 30px;
}

.mobile-shop-menu-container .mega-menu.mega-menu-plan .mega-plan-container{
    max-width: 100%;
    background-repeat: no-repeat;
    background-position: center;
    background-size: cover;
}

.mobile-shop-menu-container .mega-menu.mega-menu-plan li.mega-menu__item{
    margin-top: 20px;
}

.mobile-shop-menu-container .mega-plan-container .mega-plan-title{
    font-size: 18px;
    max-width: 205px;
    line-height: 1.5;
    margin: 0 0 5px;
    min-height: unset;
}

.mobile-shop-menu-container .mega-plan-container ul.mega-plan-items{
    display: flex;
    flex-wrap: wrap;
}

.mobile-shop-menu-container .mega-plan-container ul.mega-plan-items li{
    width: 100%;
    line-height: 1.5;
    opacity: 1;
}

.mobile-shop-menu-container .mega-plan-container .tag-new{
    position: absolute;
    top: 10px;
    margin-left: -5px;
    font-family: 'BrownPro-Regular';
    font-size: 12px;
    letter-spacing: 0;
    line-height: 14px;
    padding: 2px 3px 0;
    color: #fff;
    background-color: #85853E;
}
.mega-menu--mobile-trimester-plan{
    position: relative;
    background-color: #F6F4F2;
    padding: 20px 0 10px;
}
.mega-menu--mobile-trimester-plan .tag-new{
    position: absolute;
    top: 10px;
    margin-left: -5px;
    font-family: 'BrownPro-Regular';
    font-size: 12px;
    letter-spacing: 0;
    line-height: 14px;
    padding: 2px 3px 0;
    color: #fff;
    background-color: #85853E;
}
.mega-menu--mobile-trimester-plan p{
    color: #1F3437;
    font-family: "Copernicus-Bold";
    font-size: 18px;
    font-weight: bold;
    letter-spacing: 0;
    line-height: 24px;
    max-width: 300px;
    padding: 0 0 5px 30px;
}
.mega-menu--mobile-trimester-plan a{
    color: #85853E;
    font-family: 'BrownPro-Regular';
    font-size: 14px;
    font-weight: bold;
    letter-spacing: 0;
    line-height: 17px;
    padding-left: 30px;
}

@media (min-width: 769px){
    .mobile-top-menu-header,
    .mobile-shop-menu-container{
        display: none;
    }
}

@media (max-width: 480px){
    .mobile-shop-menu-container .mega-plan-container ul.mega-plan-items li{
        font-size: 14px;
    }
}

.mobile-shop-menu-container .mega-plan-container .plan-shop-link{
    position: relative;
    display: inline-block;
    margin-top: 15px;
    font-size: 13px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product{
    width: 100%;
    box-shadow: none;
    padding-top: 26px;
    border-top: 1px solid #e9ebec;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.mega-menu__item{
    padding-left: 0px;
    padding-right: 0px;
    margin-bottom: 22px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.mega-menu__item.mega-menu__item-bottom {
    margin-bottom: 48px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.mega-menu__item .site-nav__description{
    font-family: "BrownPro-Regular";
    font-size: 14px;
    color: #1F3337;
    opacity: 0.5;
    margin-bottom: 24px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.mega-menu__item .site-nav__link{
    padding-left: 20px;
    text-decoration: none;
    font-family: "Copernicus-Bold";
    color: #1F3437;
    font-size: 18px;
    letter-spacing: 0;
    line-height: 23px;
    position: relative;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.mega-menu__item .site-nav__link .tag-new{
    position: absolute;
    top: 0px;
    margin-left: 6px;
    font-family: 'BrownPro-Regular';
    font-size: 12px;
    letter-spacing: 0;
    line-height: 14px;
    padding: 2px 3px 0;
    color: #fff;
    background-color: #85853E;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.site-nav__footer{
    padding-top: 20px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.site-nav__footer p{
    font-size: 13px;
}

.mobile-shop-menu-container ul.mega-menu.mega-shop-product li.site-nav__footer p .spec-text{
    font-size: 13px;
    border-bottom: none;
    font-style: italic;
}

.mobile-shop-menu-container .mega-menu-plan .site-nav__link.plan-shop-link {
    margin-top: 0;
    text-decoration: none;
    display: flex;
    align-items: flex-start;
}
.mobile-shop-menu-container .mega-menu-plan .mega-plan-block {
    width: 160px;
}
.mobile-shop-menu-container .mega-menu-plan .mega-plan-content {
    margin-left: 20px;
}
.mobile-shop-menu-container .site-nav__footer {
    padding: 0;
    border-top: 1px solid #e8eaeb;
}
.mobile-shop-menu-container .site-nav__footer .find-bundle {
    font-size: 14px;
    line-height: 16px;
    letter-spacing: 0;
    opacity: 0.65;
    color: #1F3337;
    padding: 14px 0 16px;
}
.mobile-shop-menu-container .site-nav__footer .find-bundle strong {
    font-family: "Copernicus-Bold-Italic";
}
.mobile-shop-menu-container .site-nav__footer .find-bundle a {
    font-size: 14px;
    line-height: 16px;
    letter-spacing: 0;
    opacity: 0.65;
    color: #1F3337;
    margin-left: 2px;
}
.mobile-shop-menu-container .site-nav__footer .find-bundle a:hover {
    opacity: 1;
    text-decoration: none;
}
.mobile-shop-menu-container .site-nav__footer .gift-card {
    font-size: 14px;
    line-height: 16px;
    letter-spacing: 0;
    opacity: 0.65;
    color: #1F3337;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 13px 0 14px;
    border-top: 1px solid #e8eaeb;
}
.mobile-shop-menu-container .site-nav__footer .gift-card a {
    font-size: 14px;
    line-height: 16px;
    letter-spacing: 0;
    color: #868640;
    width: 139px;
    height: 37px;
    border: 1px solid #868640;
    border-radius: 18px;
    display: flex;
    justify-content: center;
    align-items: center;
    text-decoration: none;
    margin-left: 20px;
}
.mobile-shop-menu-container .site-nav__footer .gift-card a:hover {
    color: #fff;
    background-color: #868640;
}
.mobile-shop-menu-container .site-nav__footer .gift-card a .gift-card-icon {
    margin-right: 4px;
    margin-top: -2px;
}

@media (max-width: 413px) {
    .mobile-shop-menu-container .site-nav__footer .gift-card .gift-card-content {
        max-width: 184px;
    }
}
@media (max-width: 374px) {
    .mobile-shop-menu-container .site-nav__footer .gift-card a {
        margin-left: 10px;
    }
}
.mega-menu__item .shop-all {
    font-size: 15px;
    width: 100%;
    background-color: white;
    border: 1px solid #1F3337;
    color: #1F3337;
}
.mega-menu__item .shop-all:hover {
    background-color: rgba(134,134,64, 0.1);
}
</style>


