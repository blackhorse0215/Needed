            .site-nav__cart-count {
                position: absolute;
                right: -24px;
                top: 1px;
                min-width: 22px;
                height: 22px;
                overflow: hidden;
                padding-left: 1px;
                background-color: #87893e;
                border-radius: 50%;
                font-size: 11px;
                color: #fff;
                text-align: center;
                line-height: 24px;
            }
        }
    }
    .site-nav__link {
        display: inline-block;
        color: $text-dark;
        font-size: 16px;
        letter-spacing: 0;
        text-decoration: none;
        line-height: 19.2px;
        transition-property: color, background-color;
        transition-duration: .3s;
        &.site-nav__link--active {
            color: $primary;
            &:not(.site-nav__link--keyword) {
                color: $primary;
            }
        }
        &.site-nav__link--rounded {
            border-radius: 17px;
            border: 1px solid $primary;
            padding: 4px 14px;
            color: $primary;
            &:hover,
            &:focus {
                color: #fff;
                background-color: $primary;
            }
            .header--white & {
                color: #fff;
                background-color: $primary;
                &:hover {
                    background-color: darken($primary, 10%);
                }
            }
        }
        &.site-nav__link--keyword {
            display: block;
            position: relative;
            [data-keyword-context] {
                display: block;
                transition: color 300ms 100ms, transform 300ms 100ms;
            }
            &:hover {
                color: $text-dark;
                //&:not(.site-nav__link--active) {
                [data-keyword-context] {
                    color: transparent;
                    transform: translateY(-100%);
                    -ms-transform: translateY(-100%);
                    -webkit-transform: translateY(-100%);
                    -moz-transform: translateY(-100%);
                    -o-transform: translateY(-100%);
                }
                [data-icon] {
                    visibility: visible;
                    top: 50%;
                    transform: translate(-50%, -50%);
                    -ms-transform: translate(-50%, -50%);
                    -webkit-transform: translate(-50%, -50%);
                    -moz-transform: translate(-50%, -50%);
                    -o-transform: translate(-50%, -50%);
                    opacity: 1;
                }
                //}
            }
        }
        &:hover,
        &:focus {
            color: $primary;
            .site-nav__icon {
                svg,
                path {
                    fill: $primary;
                }
            }
        }
        [data-icon] {
            background-position: 50% 50%;
            background-size: contain;
            background-repeat: no-repeat;
            display: block;
            position: absolute;
            width: 30px;
            height: 30px;
            top: 100%;
            left: 50%;
            transform: translateX(-50%);
            -ms-transform: translateX(-50%);
            -webkit-transform: translateX(-50%);
            -moz-transform: translateX(-50%);
            -o-transform: translateX(-50%);
            visibility: hidden;
            opacity: 0;
            transition: top 300ms 100ms, transform 300ms 100ms, visibility 300ms, opacity 300ms 100ms;
        }
    }
}

.user {
    display: flex;
    align-items: center;
    font-size: 18px;
    line-height: normal;
    cursor: pointer;
    text-decoration: none;
    color: $text-dark;
    .user__hi {
        max-width: 200px;
        text-align: right;
        letter-spacing: 1px;
        line-height: 1.1;
    }
    .user__small-text {
        font-size: 12px;
        line-height: 1;
        letter-spacing: 0;
        color: $primary;
    }
    .user__name {
        width: 100%;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    .user__avatar {
        //display: flex;
        display: none;
        align-items: center;
        margin-left: 8px;
    }
    .user__icon {
        width: 32px;
        height: 32px;
    }
    .user__chevron {
        width: 13px;
        margin-left: 8px;
        transition: .3s all;
    }
    &.user--opened {
        .user__chevron {
            transform: rotate(180deg);
            -ms-transform: rotate(180deg);
            -webkit-transform: rotate(180deg);
            -moz-transform: rotate(180deg);
            -o-transform: rotate(180deg);
        }
    }
}

.user-menu {
    position: absolute;
    opacity: 0;
    visibility: hidden;
    top: 100%;
    right: 0;
    width: 200px;
    padding: 15px;
    background-color: #fff;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.1);
    transform: translateY(30px);
    -ms-transform: translateY(30px);
    -webkit-transform: translateY(30px);
    -moz-transform: translateY(30px);
    -o-transform: translateY(30px);
    transition-duration: .4s;
    transition-property: all;
    &::before {
        content: "";
        position: absolute;
        bottom: 100%;
        right: 27px;
        width: 0;
        height: 0;
        border-style: solid;
        border-width: 0 10px 10px 10px;
        border-color: transparent transparent #ffffff transparent;
    }
    &.user-menu--opened {
        opacity: 1;
        visibility: visible;
        transform: translateY(15px);
        -ms-transform: translateY(15px);
        -webkit-transform: translateY(15px);
        -moz-transform: translateY(15px);
        -o-transform: translateY(15px);
    }
    .user-menu__item {
        padding: 13px 0;
        &:last-child {
            padding-bottom: 0;
        }
        +.user-menu__item {
            border-top: 1px solid $light-gray-6;
        }
    }
    .user-menu__link {
        display: block;
        color: $dark-primary;
        text-decoration: none;
        transition-property: color;
        transition-duration: .3s;
        &:hover,
        &:focus {
            color: $primary;
        }
    }
}

.menu-button {
    position: relative;
    display: inline-block;
    width: 19px;
    height: 13px;
    padding: 0;
    border: none;
    background-color: transparent;
    cursor: pointer;
    &:focus {
        outline: none;
    }
    span {
        position: absolute;
        left: 0;
        display: inline-block;
        width: 100%;
        height: 1px;
        background-color: $dark-primary;
        &:nth-child(1) {
            top: 0;
            transform: rotate(0) translateZ(0);
            transition: top .3s, transform .3s .1s;
        }
        &:nth-child(2) {
            top: 6px;
            transform: translateZ(0);
            transition: .3s all;
        }
        &:nth-child(3) {
            bottom: 0;
            transform: rotate(0) translateZ(0);
            transition: bottom .3s, transform .3s .1s;
        }
    }
    &.menu-button--active {
        span {
            &:nth-child(1) {
                top: calc(50% - 1px);
                transform: rotate(45deg) translateZ(0);
            }
            &:nth-child(2) {
                opacity: 0;
            }
            &:nth-child(3) {
                bottom: 50%;
                transform: rotate(-45deg) translateZ(0);
            }
        }
    }
}

.account-link {
    display: inline-block;
    width: 32px;
    height: 32px;
}

.mobile-menu {
    display: none;
    position: fixed;
    left: 0;
    top: 55px;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    box-shadow: 0 0px 0px 1px rgba(135, 137, 62, 0.17);
    overflow-y: auto;
    // Styles for account menu (slide from right to left)
    &.mobile-menu--slide-right {
        display: block;
        right: 0;
        transform: translateX(100%);
        transition: .4s all;
        &.mobile-menu--slide-in {
            transform: translateX(0);
        }
    }
    // IMPORTANT IOS FIX
    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0,
    0,
    0);
    .mobile-menu__container {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        display: flex;
        align-items: flex-start;
    }
    .mobile-menu__hello {
        position: absolute;
        top: 30px;
        left: 50%;
        width: 100%;
        text-align: center;
        padding: 0 10px;
        font-size: 15px;
        letter-spacing: 1.3px;
        transform: translateX(-50%);
        color: $dark-primary;
        strong {
            margin-right: 5px;
            font-weight: normal;
            letter-spacing: 0;
            color: $primary;
        }
    }
    .mobile-menu__list {
        width: 100%;
        padding: 64px 30px;
        text-align: center;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        animation-name: slideUpMenu;
        animation-duration: .4s;
        animation-fill-mode: forwards;
    }
    .mobile-menu__item {
        position: relative;
        padding: 10px 0;
        width: 100%;
        margin: 6px auto;
        border-radius: 30px;
        &:hover {
            /* background-color: #f5f4f2; */
        }
        .mobile-menu__link {
            &.mob-dropdown {
                &:before {
                    position: absolute;
                    content: '';
                    background-image: url('icon-chevron-down.svg');
                    background-repeat: no-repeat;
                    background-position: center;
                    background-size: contain;
                    opacity: .5;
                    width: 20px;
                    height: 23px;
                    right: -25px;
                    top: -3px;
                    transform: rotate(0deg);
                    -webkit-transition: all .3s ease-in-out;
                    transition: all .3s ease-in-out;
                }
                &.menu-open {
                    &:before {
                        transform: rotate(180deg);
                    }
                }
                &.menu-open+.mob-sub__dropdown {
                    margin-top: 20px;
                    opacity: 1;
                    height: auto;
                    transition: .3s all;
                    visibility: visible;
                }
