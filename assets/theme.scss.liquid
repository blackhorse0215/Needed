            .more-link {
                margin-top: 50px;
                @include breakpoint(extra-small) {
                    width: 100%;
                    text-align: center;
                }
                .learn-more {
                    border-radius: 30px;
                    display: inline-block;
                    border: 2px solid #1f3337;
                    transition: .2s all;
                    &:hover {
                        background-color: #1f3337;
                        * {
                            color: #fff;
                        }
                    }
                    span {
                        color: #1f3337;
                        font-size: 16px;
                        font-weight: bold;
                        line-height: 57px;
                        padding: 0 89px;
                        display: inline-block;
                        @include breakpoint(extra-small) {
                            line-height: 48px;
                            padding: 0 63px;
                        }
                    }
                }
            }
        }
        .item__title {
            font-size: 28px;
            line-height: 42px;
            letter-spacing: 0;
            @include breakpoint(extra-small) {
                font-size: 22px;
                line-height: 30px;
                width: 100%;
                text-align: center;
            }
        }
        .item__sub_title {
            font-size: 28px;
            line-height: 42px;
            @include breakpoint(extra-small) {
                font-size: 24px;
                line-height: 36px;
                width: 100%;
                text-align: center;
            }
        }
        .item__description {
            line-height: 30px;
            color: $dark-primary;
            margin-bottom: 35px;
            margin-top: 35px;
            padding-right: 85px;
            font-size: 18px;
            @include breakpoint(extra-small) {
                font-size: 17px;
                line-height: 28px;
                padding-right: 0;
                margin-bottom: 30px;
            }
        }
        @media (min-width: 992px) {
            margin-top: 0;
            .item__image_wrapper {
                padding-bottom: 0;
                img {
                    transform: none;
                    position: static;
                    //height: 600px;
                    // margin: 0 -10px;
                    &.placehoder-img {
                        height: auto;
                    }
                }
            }
            .item__image {
                //padding-left: 0;
            }
            &:nth-child(2n) {
                .item__image {
                    order: 1;
                    padding-left: 10px;
                    padding-right: 0;
                }
                .item__content {
                    order: 0;
                }
            }
            .item__content {
                margin-top: 0;
                //padding: 0 60px;
            }
        }
    }
}

