/*================ Mixins================*/

//mixins, like breakpoint or other...
@mixin breakpoint($point) {
    @if $point==extra-large {
        @media (max-width: 1700px) {
            @content;
        }
    }
    @if $point==large {
        @media (max-width: 1500px) {
            @content;
        }
    }
    @else if $point==medium {
        @media (max-width: 1199px) {
            @content;
        }
    }
    @else if $point==small {
        @media (max-width: 991px) {
            @content;
        }
    }
    @else if $point==extra-small {
        @media (max-width: 767px) {
            @content;
        }
    }
    @else if $point==small-tablet-portrait {
        @media (max-width: 575px) {
            @content;
        }
    }
    @else if $point==mobile-landscape {
        @media (max-width: 480px) {
            @content;
        }
    }
    @else if $point==mobile {
        @media (max-width: 380px) {
            @content;
        }
    }
    @else if $point==no-mobile {
        @media (min-width: 1200px) {
            @content;
        }
    }
    @else if $point==full-mobile-landscape {
        @media (max-width: 800px) and (max-height: 450px) {
            @content;
        }
    }
    @else if $point==small-height {
        @media (max-height: 800px) {
            @content;
        }
    }
}

@mixin placeholder {
    &::-webkit-input-placeholder {
        @content;
    }
    &:-moz-placeholder {
        @content;
    }
    &::-moz-placeholder {
        @content;
    }
    &:-ms-input-placeholder {
        @content;
    }
}

@mixin ie () {
    @media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) {
        @content
    }
}

@mixin me {
    @supports (-ms-ime-align: auto) {
        @content
    }
}


