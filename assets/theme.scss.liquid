.product-fixed-form {
    position: fixed;
    top: 86px; // header height
    left: 0;
    width: 100%;
    padding: 10px 0;
    background-color: #fff;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.1);
    transform: translateY(-100%);
    opacity: 0;
    visibility: hidden;
    transition-property: opacity, visibility, transform;
    transition-duration: .3s;
    transition-timing-function: ease-out;
    z-index: 15;
    .container {
        max-width: 1170px;
    }
    @include breakpoint(medium) {
        top: 45px;
        padding-left: 10px;
        padding-right: 10px;
    }
    @include breakpoint(small) {
        padding-right: 0;
        padding-left: 0;
    }
    &.product-fixed-form--shown {
        transform: translateY(0);
        opacity: 1;
        visibility: visible;
    }
    .row {
        align-items: center;
    }
    .product-fixed-form__title {
        font-family: $base-font-bold;
        font-weight: normal;
        font-size: 18px;
        text-align-last: left;
        color: #1f3337;
        @include breakpoint(medium) {
            padding: 0 5px;
        }
        @include breakpoint(extra-small) {
            font-size: 15px;
            line-height: 20px;
        }
        {% comment %} @include breakpoint(small-tablet-portrait) {
            max-width: calc(100% - 185px);
            flex: auto;
        }
        @media (max-width: 374px) {
            max-width: calc(100% - 115px);
        } {% endcomment %}
    }
    .product-fixed-form__action {
        display: flex;
        justify-content: flex-end;
        /* padding-right: 0; */
        .button {
            /* text-transform: lowercase; */
            min-width: 165px;
            .sticky-buy-now {
                padding-right: 10px;
            }
            @include breakpoint(extra-small) {
                font-size: 15px;
                min-height: initial;
                padding: 14px 25px;
            }
            @media (max-width: 374px) {
                min-width: 115px;
                padding: 10px 8px;
            }
        }
        @include breakpoint(medium) {
            padding: 0 5px;
        }
        {% comment %} @include breakpoint(small-tablet-portrait) {
            max-width: 185px;
        }
        @media (max-width: 374px) {
            max-width: 115px;
        } {% endcomment %}
    }
}

.addition-contents {
    padding: 65px 0 0;
    max-width: calc(100% - 200px);
    margin: 0 auto;
    .contents {
        position: relative;
        text-indent: 55px;
        font-size: 22px;
        font-style: normal;
        line-height: 38px;
        &:before {
            content: open-quote;
            position: absolute;
            left: 0;
            opacity: 0;
            visibility: hidden;
        }
        &:after {
            content: close-quote;
            position: absolute;
            top: -65px;
            left: 0;
            font-family: "Copernicus-Book", sans-serif;
            font-size: 90px;
            line-height: 1;
            letter-spacing: 3.8px;
            transform: rotate(180deg);
            color: #85853e;
        }
        @include breakpoint(small-tablet-portrait) {
            text-indent: 40px;
            font-size: 17px;
            line-height: 32px;
            &:after {
                top: -55px;
                font-size: 75px;
                line-height: 1;
                letter-spacing: 3.8px;
            }
        }
    }
    .addition-author {
        margin-top: 30px;
        font-size: 16px;
        line-height: 24px;
        font-family: "BrownPro-Bold", sans-serif;
        font-weight: normal;
        letter-spacing: 0.6px;
