    .based-level-slider__image {
        &.based-level-slider__image--mobile {
            @media (min-width: 992px) {
                display: none;
            }
            margin-left: 30px;
            margin-right: 22px;
            max-height: 250px;
            overflow: hidden;
            img {
                max-height: 100%;
                object-fit: contain;
                height: 250px;
                @include breakpoint(mobile-landscape) {
                    height: auto;
                }
            }
            @include breakpoint(small) {
                margin-left: auto;
                margin-right: auto;
                max-height: initial;
                max-width: 320px;
                width: 100%;
                img {
                    height: initial;
                }
            }
        }
        &.based-level-slider__image--desktop {
            @include breakpoint(small) {
                display: none;
            }
        }
    }
    .based-level-slider__title {
        // display: flex;
        margin-bottom: 100px;
        // color: $dark-primary;
        // flex-flow: column nowrap;
    }
    .based-level-slider__keyword {
        // color: $primary;
        font-size: 32px;
        line-height: 38px;
        letter-spacing: 0;
        max-width: 900px;
        margin: 0 auto;
        text-align: center;
        font-family: "Copernicus-Bold", sans-serif;
    }
    @include breakpoint(small) {
        .based-level-slider__heading {
            justify-content: space-between;
        }
        .based-level-slider__title {
            margin-bottom: 30px;
        }
        .based-level-slider__keyword {
            font-size: 24px;
            line-height: 28px;
            letter-spacing: 0;
        }
    }
    .based-level-slider__slider {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        -ms-transform: translateY(-50%);
        -webkit-transform: translateY(-50%);
        -moz-transform: translateY(-50%);
        -o-transform: translateY(-50%);
        .utd-target {
            border-radius: 7px;
            border: none;
            box-shadow: none;
            height: 215px;
            margin-left: 30px;
            .utd-pips-vertical {
                padding: 0 10px;
                height: 300px;
                top: -46px;
                left: 0;
            }
            @include breakpoint(small-tablet-portrait) {
                height: 190px;
                .utd-pips-vertical {
                    height: 270px;
                    top: -30px;
                }
            }
        }
        .utd-connects {
            border-radius: 7px;
        }
        .utd-handle {
            box-shadow: 0 4px 10px 0 rgba(0, 0, 0, 0.27);
            width: 42px;
            height: 42px;
            border: none;
            border-radius: 50%;
            top: -15px;
            // right: -24px !important;
            left: -12px;
            cursor: grab;
            &:before {
                position: absolute;
                content: '';
                background-image: url('based-level-scroll.png');
                background-repeat: no-repeat;
                background-size: contain;
                background-position: center;
                background-color: transparent;
                padding: 15.5px;
                top: 5px;
                left: -2px;
            }
            &:after {
                display: none;
            }
        }
        .utd-marker {
            display: none;
        }
        .utd-value {
            color: $text-dark;
            line-height: 16px;
            letter-spacing: 0;
            font-size: 14px;
            padding-left: 0;
            &.utd-value-large {
                font-size: 0;
                line-height: 0;
                &[data-value="0"],
                &:last-of-type {
                    display: block;
                    opacity: 1;
                    outline: none;
                    font-size: 14px;
                    line-height: 16px;
                    width: 55px;
                    white-space: initial;
                    left: -15px;
                    &:before {
                        position: relative;
                    }
                }
                &:before {
                    content: "";
                    position: absolute;
                    left: -6px;
                    top: 0;
                    width: 10px;
                    height: 10px;
                    border-radius: 30px;
                    background-color: #fff;
                }
            }
            a {
                color: inherit;
                text-decoration: none;
                transition: all ease-in 0.3s;
                &:hover {
                    color: #87893e;
                }
            }
        }
        .utd-pips-horizontal {
            .utd-value-large {
                /*opacity:  0;
				display: none;*/
                font-size: 0;
                line-height: 0;
                &::before {
                    content: "";
                    position: absolute;
                    left: 0;
                    top: 0;
                    width: 8px;
                    height: 8px;
                    border-radius: 2px;
                    background-color: $primary;
                    transform: rotate(45deg);
                    -ms-transform: rotate(45deg);
                    -webkit-transform: rotate(45deg);
                    -moz-transform: rotate(45deg);
                    -o-transform: rotate(45deg);
                }
                &[data-value="0"] {
                    display: block;
                    opacity: 1;
                    outline: none;
                    font-size: inherit;
                    line-height: inherit;
                    &::before {
                        display: none;
                    }
                }
                &:last-of-type {
                    display: block;
                    opacity: 1;
                    outline: none;
                    font-size: inherit;
                    line-height: inherit;
                    &::before {
                        display: none;
                    }
                }
            }
        }
        @include breakpoint(small) {
            top: initial;
            transform: translateY(-50%);
            -ms-transform: translateY(-50%);
            -webkit-transform: translateY(-50%);
            -moz-transform: translateY(-50%);
            -o-transform: translateY(-50%);
            bottom: 0;
        }
        @include breakpoint(small-tablet-portrait) {
            top: 600px;
            transform: initial;
            -ms-transform: initial;
            -webkit-transform: initial;
            -moz-transform: initial;
            -o-transform: initial;
            bottom: initial;
        }
    }
}

