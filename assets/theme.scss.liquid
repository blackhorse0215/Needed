// Faq
.faq {
    padding-top: 44px;
    padding-bottom: 53px;
    .faq__row {
        margin-top: 80px;
    }
    .faq__title {
        display: flex;
        justify-content: space-between;
        align-items: center;
        //margin-bottom: 21px;
        font-family: $heading-font-extra-bold;
        font-weight: normal;
        font-size: 20px;
        letter-spacing: 1.2px;
        span {
            cursor: pointer;
        }
        .faq__title-toggler {
            display: inline-block;
            width: 30px;
            height: 27px;
            flex-shrink: 0;
            background-image: url('icon-chevron-down.svg');
            background-repeat: no-repeat;
            background-position: 50% 50%;
            background-size: contain;
            transition: .3s all;
            &.accordion__state--active {
                transform: rotate(180deg);
            }
        }
        @include breakpoint(small) {
            font-size: 18px;
        }
    }
    .faq__questions {
        padding-left: 45px;
        @include breakpoint(extra-small) {
            padding-left: 20px;
        }
    }
    .faq__container {
        +.faq__container {
            margin-top: 40px;
        }
    }
}

.faq-item {
    padding-top: 18px;
    padding-bottom: 15px;
    +.faq-item {
        border-top: 2px solid #e3e3e3;
    }
    .faq-item__question {
        display: flex;
        justify-content: space-between;
        align-items: center;
        font-family: $base-font-bold;
        font-size: 18px;
        font-weight: normal;
        color: $primary;
        letter-spacing: 1px;
        transition-property: color;
        transition-duration: .3s;
        span {
            cursor: pointer;
        }
        &.active {
            //color: $dark-primary;
            span:first-child {
                &::after {
                    content: "*";
                    color: $dark-primary;
                }
            }
        }
        @include breakpoint(small) {
            font-size: 16px;
        }
    }
    .faq-item__answer {
        margin-top: 10px;
        font-size: 16px;
        color: $text-light;
        line-height: 1.75;
        a {
            color: $primary;
            text-decoration: underline;
            &:hover {
                text-decoration: none;
            }
        }
    }
    .faq-item__toggler {
        display: inline-block;
        width: 18px;
        height: 18px;
        flex-shrink: 0;
        margin-right: 7px;
        position: relative;
        transition: .3s all;
        &::before,
        &::after {
            content: "";
            position: absolute;
            background-color: currentColor;
            transition: .3s all;
        }
        &::before {
            width: 100%;
            height: 1px;
            left: 0;
            top: 50%;
            transform: translateY(-50%);
        }
        &::after {
            height: 100%;
            width: 1px;
            left: 50%;
            transform: translateX(-50%);
        }
        &.accordion__state--active {
            transform: rotate(90deg);
            &::before {
                transform: translateY(-50%) rotate(90deg);
            }
        }
    }
}

