//Subscription popup
@keyframes openPopup {
    from {
        z-index: -1;
    }
    to {
        z-index: 55;
    }
}

@keyframes closePopup {
    from {
        z-index: 55;
    }
    to {
        z-index: -1;
    }
}

.subscription-popup-wrapper {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    display: flex;
    width: 100%;
    z-index: -1;
    animation: closePopup 0.5s linear forwards;
    &.opened {
        z-index: 55;
        animation: openPopup 0.5s linear forwards;
        .subscription-popup-overlay {
            visibility: visible;
            opacity: 0.54;
            z-index: 55;
        }
        .subscription-popup {
            visibility: visible;
            opacity: 1;
            z-index: 56;
        }
    }
}

.subscription-popup-overlay {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background-color: #1f3537;
    visibility: hidden;
    opacity: 0;
    transition: all 0.4s ease-out;
    z-index: -1;
    -webkit-transform: translate3d(0, 0, 0);
    cursor: pointer;
}

.subscription-popup {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-self: flex-start;
    //margin: auto;
    visibility: hidden;
    opacity: 0;
    transition: all 0.4s ease-out;
    z-index: -1;
    //height: 100vh;
    height: auto;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0, 0, 0);
    .subscription-popup__content-wrapper {
        max-width: 1288px;
        width: 100%;
        margin: auto;
        padding: 60px;
    }
    .subscription-popup__content {
        position: relative;
        text-align: center;
        background-color: #fff;
        padding: 90px;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
        outline: 2px solid #fff;
        outline-offset: -45px;
    }
    .subscription-popup__close-wrapper {
        position: absolute;
        top: 18px;
        right: 18px;
    }
    .subscription-popup__close {
        position: relative;
        width: 16px;
        height: 16px;
        display: block;
        cursor: pointer;
        transition: color 0.4s ease-out;
        &:before,
        &:after {
            content: '';
            position: absolute;
            left: 50%;
            top: 50%;
            width: 100%;
            height: 2px;
            background-color: $text-dark;
            transition: background-color 250ms;
        }
        &:after {
            transform: translate(-50%, -50%) rotate(45deg);
        }
        &:before {
            transform: translate(-50%, -50%) rotate(-45deg);
        }
        &:hover {
            &:after,
            &:before {
                background-color: #fff;
            }
        }
    }
    .subscription-popup__heading {
        margin-bottom: 21px;
        font-size: 32px;
        font-weight: bold;
        font-style: normal;
        font-stretch: normal;
        line-height: normal;
        letter-spacing: normal;
        color: #1f3537;
    }
    .subscription-popup__description {
        margin: 0 auto;
        margin-bottom: 24px;
        max-width: 500px;
        font-size: 18px;
        color: #1f3537;
    }
    .subscription-popup__form {
        width: 100%;
    }
    .subscribe-form {
        margin: 0 auto;
        max-width: 492px;
    }
    .subscription-popup__close-bottom {
        display: block;
        margin-top: 17px;
        font-size: 15px;
        cursor: pointer;
        transition: color 0.4s ease-out;
        color: #1f3537;
        &:hover {
            color: #fff;
        }
    }
    .subscription-popup__subscribe-messages {
        margin: 0 auto;
        margin-top: 5px;
        max-width: 492px;
        font-size: 15px;
        text-align: left;
        position: relative;
        min-height: 17px;
    }
    .subscription-popup__subscribe-error {
        color: red;
        opacity: 1;
        visibility: visible;
        transition: opacity 0.3s ease-out;
        position: absolute;
        top: 0;
        left: 0;
        &.hidden {
            display: block;
            opacity: 0;
            visibility: hidden;
        }
    }
    .subscription-popup__subscribe-success {
        color: $primary;
        opacity: 1;
        visibility: visible;
        transition: opacity 0.3s ease-out;
        position: absolute;
        top: 0;
        left: 0;
        &.hidden {
            display: block;
            opacity: 0;
            visibility: hidden;
        }
    }
    @include breakpoint(small) {
        .subscription-popup__content-wrapper {
            padding-left: 8px;
            padding-right: 8px;
        }
    }
    @include breakpoint(extra-small) {
        .subscription-popup__content {
            outline: 2px solid #fff;
            outline-offset: -12px;
            padding-left: 36px;
            padding-right: 36px;
        }
        .subscription-popup__close-wrapper {
            display: none;
        }
        .subscription-popup__heading {
            font-size: 16px;
        }
        .subscription-popup__description {
            font-size: 13px;
        }
        .subscription-popup__close-bottom {
            font-size: 12px;
