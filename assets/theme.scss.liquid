.product-slider {
    margin-top: 50px;
    margin-bottom: 60px;
    .container {
        padding-top: 90px;
        padding-bottom: 120px;
        // padding: 90px 0 120px;
    }
    @include breakpoint(small) {
        margin-top: 0;
        .container {
            padding-top: 50px;
            padding-bottom: 80px;
        }
    }
    .product-slider__content {
        [data-content] {
            position: relative;
            color: $text-dark;
            line-height: 28px;
            letter-spacing: 0.39px;
            display: none;
            a {
                color: $primary;
                text-decoration: underline;
                &:hover {
                    text-decoration: none;
                }
            }
            p {
                margin: 0;
            }
            &.shown {
                display: block;
                animation-name: slideUp;
                animation-duration: .5s;
                animation-fill-mode: forwards;
            }
            &:not(:last-child) {
                //margin-bottom: 20px;
            }
            @include breakpoint(extra-small) {
                font-size: 15px;
                line-height: 25px;
                letter-spacing: 0.32px;
                transform: translateY(0);
            }
        }
    }
    .product-slider__heading {
        display: flex;
        flex-flow: row nowrap;
        justify-content: flex-start;
        align-items: center;
        @include breakpoint(extra-small) {
            margin-bottom: 10px;
        }
    }
    .product-slider__image {
        &.product-slider__image--mobile {
            @media (min-width: 992px) {
                display: none;
            }
            margin-left: 30px;
            margin-right: 22px;
            max-height: 250px;
            overflow: hidden;
            img {
                max-height: 100%;
                object-fit: contain;
                height: 250px;
                @include breakpoint(mobile-landscape) {
                    height: auto;
                }
            }
        }
        &.product-slider__image--desktop {
            @include breakpoint(small) {
                display: none;
            }
        }
    }
    .product-slider__title {
        display: flex;
        margin-bottom: 30px;
        color: $dark-primary;
        flex-flow: column nowrap;
    }
    .product-slider__keyword {
        color: $primary;
        font-size: 32px;
        line-height: 50px;
        letter-spacing: 2.3px;
    }
    @include breakpoint(small) {
        .product-slider__heading {
            justify-content: space-between;
        }
    }
    @include breakpoint(extra-small) {
        .product-slider__title {
            font-size: 13px;
        }
        .product-slider__keyword {
            font-size: 22px;
            line-height: normal;
            letter-spacing: 1.59px;
        }
    }
    .product-slider__slider {
        margin-top: 20px;
        .utd-target {
            border-radius: 7px;
            border: none;
            box-shadow: none;
            height: 14px;
        }
        .utd-connects {
            border-radius: 7px;
        }
        .utd-handle {
            box-shadow: 0 4px 10px 0 rgba(0, 0, 0, 0.27);
            width: 42px;
            height: 42px;
            border: none;
            border-radius: 50%;
            top: -15px;
            //right: -23px !important;
            right: -24px !important;
            cursor: grab;
            &:after,
            &:before {
                display: none;
            }
        }
        .utd-marker {
            display: none;
        }
        .utd-value {
            color: $text-dark;
            line-height: 24px;
            letter-spacing: 0.39px;
            a {
                color: inherit;
                text-decoration: none;
                transition: all ease-in 0.3s;
                &:hover {
                    color: #87893e;
                }
            }
        }
        .utd-pips-horizontal {
            .utd-value-large {
                /*opacity:  0;
				display: none;*/
                font-size: 0;
                line-height: 0;
                &::before {
                    content: "";
                    position: absolute;
                    left: 0;
                    top: 0;
                    width: 8px;
                    height: 8px;
                    border-radius: 2px;
                    background-color: $primary;
                    transform: rotate(45deg);
                }
                &[data-value="0"] {
                    display: block;
                    opacity: 1;
                    outline: none;
                    font-size: inherit;
                    line-height: inherit;
                    &::before {
                        display: none;
                    }
                }
                &:last-of-type {
                    display: block;
                    opacity: 1;
                    outline: none;
                    font-size: inherit;
                    line-height: inherit;
                    &::before {
                        display: none;
                    }
                }
            }
        }
        @include breakpoint(small) {
            margin-top: 20px;
            padding: 0 15px;
            .utd-target {
                border-radius: 7px;
                border: none;
                box-shadow: none;
                height: 10px;
            }
            .utd-handle {
                width: 20px;
                height: 20px;
                top: -6px;
                right: -11px !important;
            }
            .utd-value {
                max-width: 60px;
                white-space: normal;
                word-wrap: normal;
                text-overflow: clip;
                font-size: 13px;
                line-height: 15px;
                letter-spacing: 0.28px;
            }
            .utd-pips-horizontal {
                .utd-value-large {
                    &::before {
                        left: -2px;
                    }
                    &[data-value="0"] {
                        font-size: 12px;
                        transform: translateX(0);
                    }
                    &:last-of-type {
                        font-size: 12px;
                        transform: translateX(-100%);
                    }
                }
            }
        }
    }
}

