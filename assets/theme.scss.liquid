// Cart page
.template-cart {
    padding-top: 170px;
    .cart-empty {
        margin-bottom: 30px;
        text-align: center;
        h1 {
            margin-bottom: 25px;
        }
        p {
            +p {
                margin-top: 5px;
            }
        }
        a {
            color: $primary;
        }
    }
    @include breakpoint(small) {
        padding-top: 130px;
    }
}

.trust-indicators {
    display: flex;
    justify-content: flex-end;
    font-size: 14px;
    color: #1f3337;
    opacity: .65;
    line-height: 21px;
    margin-top: 20px;
    li:not(:first-child) {
        position: relative;
        margin-left: 15px;
        padding-left: 20px;
        &:before {
            position: absolute;
            content: '\2022';
            left: 0;
            top: -1px;
            width: 5px;
            height: 5px;
            font-size: 8px;
        }
    }
    @include breakpoint(extra-small) {
        margin-top: 15px;
        justify-content: initial;
        display: block;
        li {
            display: inline-block;
        }
    }
}

.cart-item {
    .cart-item__image-td {
        width: 130px;
    }
    .cart-item__image-wrapper {
        padding-right: 30px;
        a {
            display: block;
        }
    }
    .cart-item__title {
        display: inline-block;
        margin-bottom: 5px;
        color: $primary;
        transition: .3s all;
        &:hover {
            text-decoration: underline;
        }
    }
    .cart-item__option {
        margin-top: 4px;
        font-size: 15px;
    }
    .cart-item__remove {
        display: inline-block;
        margin-top: 15px;
        padding: 6px 10px;
        border: 1px solid $dark-primary;
        background-color: $dark-primary;
        color: #fff;
        font-size: 16px;
        letter-spacing: 0.4px;
        text-transform: uppercase;
        text-align: center;
        transition: .3s all;
        &:hover {
            background-color: #fff;
            color: $dark-primary;
        }
    }
    .cart-item__qty {
        width: 60px;
        padding: 10px 8px;
        border: 1px solid $dark-primary;
        font-size: 18px;
        line-height: 1.15;
        &:focus {
            border-color: $primary;
        }
    }
    @include breakpoint(extra-small) {
        position: relative;
        padding-bottom: 40px;
        +.cart-item {
            margin-top: 40px;
        }
        .cart-item__image-td {
            text-align: left;
            &::before {
                display: none;
            }
        }
        .cart-item__image-wrapper {
            /* padding-right: 0;
            width: 130px;
            margin-top: 15px; */
        }
        .cart-item__remove {
            position: absolute;
            right: 0;
            bottom: 0;
        }
    }
}

.cart-title {
    font-size: 32px;
    line-height: 38px;
    letter-spacing: 0;
    @include breakpoint(extra-small) {
        font-size: 24px;
        line-height: 28px;
    }
}

.cart-form {
    margin-top: 50px;
    .cart-form__footer {
        display: flex;
        flex-wrap: wrap;
        margin: 30px 0 80px;
        text-align: right;
    }
    .icon--wrapper {
        width: 60%;
        .icons__row {
            display: flex;
            justify-content: center;
            .icons__col {
                .icons__item {
                    display: flex;
                    align-items: center;
                    .icons__item-icon {
                        position: relative;
                        display: inline-block;
                        width: 32px;
                        height: 32px;
                        background-repeat: no-repeat;
                        background-size: contain;
                        background-position: 50% 50%;
                        padding: 16px;
                        &:before {
                            content: "";
                            position: absolute;
                            left: 0;
                            top: 0;
                            z-index: 5;
                            width: 100%;
                            height: 100%;
                            opacity: 0;
                            background-repeat: no-repeat;
                            background-size: contain;
                            background-position: 50% 50%;
                            background-color: #fff;
                            transition: .3s all;
                        }
                    }
                    .icons__item-title {
                        display: block;
                        font-family: "BrownPro-Regular", sans-serif;
                        color: #213637;
                        font-size: 13px;
                        line-height: 15px;
                        padding-left: 13px;
                        text-align: left;
                    }
                    .icons__item-icon--icon_1_hover {
                        &:before {}
                    }
                    .icons__item-icon--icon_2_hover {
                        &:before {}
                    }
                    .icons__item-icon--icon_3_hover {
                        &:before {}
                    }
                }
            }
            .have_ques {
                color: #1f3337;
                opacity: .65;
                line-height: 24px;
                font-size: 16px;
                width: 100%;
                text-align: left;
                margin-top: 40px;
                span {}
                a {
                    color: #1f3337;
                    line-height: 24px;
                    font-size: 16px;
                    /* text-decoration: none; */
                    &:hover {
                        color: #1f3337;
                        /* text-decoration: none; */
                    }
                }
            }
        }
        @include breakpoint(extra-small) {
            width: 100%;
            order: 2;
            display: flex;
            flex-direction: column;
            .icons__row {
                display: block;
                &:first-child {
                    order: 2;
                    padding-top: 25px;
                }
                .icons__col {
                    margin-top: 25px;
                }
                .have_ques {
                    margin-top: 25px;
                    text-align: center;
                }
            }
        }
    }
    .cart-form__notes,
    .cart-form__action {
        // width: 50%;
        width: 40%;
    }
    .cart-form__notes {
        padding-right: 40px;
        text-align: left;
    }
    .cart-form__action {
        margin-left: auto;
    }
    .cart-form__notes-label {
        display: inline-block;
        margin-bottom: 15px;
    }
    .cart-form__total-price {
        font-family: $base-font-bold;
        font-size: 20px;
        line-height: 24px;
        font-weight: 600;
    }
    .cart-form__total-price-value {
        padding-left: 25px;
    }
    .cart-form__shipping {
        margin: 10px 0;
        font-size: 14px;
