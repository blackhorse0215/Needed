                transform: translate(-50%, -50%);
                width: 100%;
                object-fit: cover;
                height: auto;
            }

            @media (max-width: 991px) {
                padding-bottom: 90%;
            }

            @include breakpoint(extra-small) {
                padding-bottom: 100%;
                img {
                    left: initial;
                    top: initial;
                    transform: initial;
                }
            }
        }
        .item__content {
            margin-top: 20px;
            display: flex;
            flex-flow: column nowrap;
            align-items: flex-start;
            justify-content: center;
            @include breakpoint(extra-small) {
                flex: none;
                order: 2;
                margin-top: 4px;
            }
            @media screen and (max-width: 480px) {
                align-items: center;
            }
            .thumbnail-imgs {
                .thumbnail-img {
                    max-width: 140px;
                    display: inline-block;
                    position: relative;
                    &:first-child {
                        margin-right: 100px;
                        &:after {
                            position: absolute;
                            content: '&';
                            top: 50%;
                            transform: translateY(-50%);
                            right: -50px;
                        }
                    }
                    img {}
                }
                @include breakpoint(small-tablet-portrait) {
                    .thumbnail-img {
                        max-width: 110px;
                        &:first-child {
                            margin-right: 85px;
                        }
                    }
                }
                @include breakpoint(mobile-landscape) {
                    .thumbnail-img {
                        max-width: 95px;
                        &:first-child {
                            margin-right: 65px;
                            &:after {
                                right: -38px;
                            }
