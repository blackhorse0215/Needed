/*================ UI ================*/

.button {
    display: inline-block;
    min-width: 224px;
    min-height: 49px;
    padding: 16px 25px;
    border: none;
    border-radius: 25px;
    box-shadow: none;
    font-family: $base-font-bold;
    font-weight: normal;
    font-size: 15px;
    line-height: 1;
    text-align: center;
    text-decoration: none;
    transition-property: color, background-color;
    transition-duration: .3s;
    &.button--primary {
        color: #fff;
        background-color: $dark-primary;
        &:hover,
        &:focus {
            background-color: $text-light;
        }
        span.dot {
            font-size: 16px;
            padding: 0 10px 0 5px;
            vertical-align: middle;
        }
    }
    &.button--secondary {
        background-color: #e5e5e5;
        &:hover,
        &:focus {
            background-color: darken(#e5e5e5, 15%);
        }
    }
    &.button--small {
        min-width: 155px;
    }
    &.button--rounded {
        border-radius: 25px;
    }
    // Disabled
    &.disabled,
    &[disabled] {
        cursor: default;
        background-color: $text-light;
        &:hover,
        &:focus {}
    }
}

.checkbox,
input[type="checkbox"] {
    &:checked {}
    +label {}
}

.input {
    width: 100%;
    border: none;
    padding: 0;
    color: $text-light;
    font-size: 18px;
    line-height: 28px;
    letter-spacing: 0.34px;
    box-shadow: inset 0 -1px 0 0 #2f2f2f;
    border-radius: 0;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    transition: box-shadow 250ms;
    &.input--error {
        box-shadow: inset 0 -1px 0 0 red;
    }
    &::-ms-clear {
        display: none;
    }
    &:focus {
        box-shadow: inset 0 -2px 0 0 $primary;
        &+label {
            color: $primary;
        }
    }
    @include placeholder() {
        color: #ccc;
    }
}

.input+label {
    transition: color 250ms;
}

.input-prelabel {
    position: relative;
    display: flex;
    width: 100%;
    align-items: center;
    padding-top: 28px;
    padding-bottom: 6px;
    box-shadow: inset 0 -1px 0 0 #2f2f2f;
    transition: box-shadow 250ms;
    .input-prelabel__prefix {
        flex-shrink: 0;
        color: #2d2d2d;
    }
    label {
        position: absolute;
        top: 0;
        left: 0;
    }
    input,
    .input {
        box-shadow: none;
        padding-bottom: 0;
    }
    &.focused {
        box-shadow: inset 0 -2px 0 0 $primary;
    }
}

.input-radio {
    label {}
    input[type=radio] {}
    input[type=radio]:checked {
        +label {}
    }
}

.checkbox,
input[type="checkbox"] {
    width: 16px;
    height: 16px;
    border: 1px solid $dark-primary;
    background-color: #fff;
    box-shadow: none;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    background-size: contain;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    cursor: pointer;
    &:checked {
        background-image: url('icon-checkbox.svg');
    }
}

.textarea {
    width: 100%;
    height: 150px;
    padding: 10px;
    border: 1px solid #3e3e3e;
    font-size: 16px;
    box-shadow: none !important;
    border-radius: 0;
    resize: none;
    transition-property: border-color;
    transition-duration: .3s;
    -webkit-appearance: none;
    appearance: none;
    @include placeholder {
        color: #e5e5e5;
    }
    &:focus {
        border-color: $primary;
    }
}

.pagination {
    margin-top: 50px;
    text-align: center;
    span {
        display: inline-block;
        margin: 0 5px;
    }
    a {
        color: $primary;
        text-decoration: none;
        transition-duration: .3s;
        transition-property: color;
        &:hover,
        &:focus {
            color: lighten($primary, 15%);
        }
    }
}

.select {
    width: 100%;
    height: 27px;
    border: none;
    border-bottom: 1px solid $text-dark;
    background-color: transparent;
    padding-top: 3px;
    padding-bottom: 3px;
    padding-right: 25px;
    font-family: $base-font-bold;
    font-weight: normal;
    font-size: 18px;
    line-height: 1.15;
    box-shadow: none;
    border-radius: 0;
    background-image: url('icon-chevron-down.png');
    background-repeat: no-repeat;
    background-size: 12px;
    background-position: top 50% right 5px;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    color: $text-dark;
    cursor: pointer;
    transition: border-bottom-color 250ms, color 250ms;
    &:disabled {
        opacity: 0.5;
    }
    &.select--error {
        color: red;
        border-bottom-color: red;
        option {
            color: $text-dark;
            &:disabled {
                color: #ccc;
            }
        }
    }
}

.slick-dots {
    display: flex;
    justify-content: center;
    margin-top: 40px;
    margin-left: auto;
    margin-right: auto;
    li {
        margin: 0 3px;
        display: inline-flex;
        button {
            padding: 0;
            background-color: transparent;
            width: 11px;
            height: 11px;
            border-radius: 50%;
            border: 1px solid $dark-primary;
            font-size: 0;
            line-height: 0;
            color: transparent;
            transition-property: background-color;
            transition-duration: .3s;
        }
        &.slick-active {
            button {
                background-color: $dark-primary;
            }
        }
    }
    @include breakpoint(extra-small) {
        margin-top: 20px;
    }
}

.tag {
    margin: 5px;
    a,
    .tag__link {
        display: inline-block;
        padding: 4px 17px;
        text-decoration: none;
        line-height: 1.1;
        letter-spacing: 0.3px;
        font-size: 14px;
        color: $primary;
        border-radius: 12px;
        background-color: #fff;
        border: 1px solid $primary;
        transition: .3s all;
        &:hover,
        &:focus {
            background-color: darken($primary, 10%);
            color: #fff;
        }
    }
    &.tag--active {
        a,
        .tag__link {
            background-color: $primary;
            color: #fff;
        }
    }
}


/*================ Blocks ================*/


/*============================================================================
  Responsive tables, defined with .responsive-table on table element.
==============================================================================*/

.responsive-table {
    width: 100%;
    text-align: left;
    tr {
        border-bottom: 1px solid $gray;
        @include breakpoint(extra-small) {
            border-bottom: none;
        }
    }
    th {
        padding: 20px 0;
        font-family: $base-font-bold;
        font-size: 16px;
        color: $text-dark;
        &:last-child {
            text-align: right;
        }
    }
    th,
    td {
        @include breakpoint(small) {}
    }
    td {
        padding: 20px 0;
        vertical-align: middle;
        a {
            text-decoration: none;
            color: $text-dark;
            transition: .3s all;
        }
        &:last-child {
            text-align: right;
        }
    }
}

@include breakpoint(extra-small) {
    .responsive-table {
        width: 100%;
        thead {
            display: none;
        }
        tr {
            display: block;
        }
        // IE9 table layout fixes
        tr,
        td {
            float: left;
            clear: both;
            width: 100%;
        }
        th,
        td {
            display: block;
            text-align: right;
            padding: 15px 0;
            margin: 0;
            &.small--hide {
                display: none;
            }
        }
        td[data-label="Quantity"] {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        td::before {
            content: attr(data-label);
            float: left;
            font-family: $base-font-bold;
            color: $text-dark;
            text-align: left;
            padding-right: 10px;
        }
    }
    // Add a keyline between rows
    .responsive-table-row+.responsive-table-row,
    tfoot>.responsive-table-row:first-child {
        position: relative;
        margin-top: 15px;
        padding-top: 15px;
        &::after {
            content: '';
            display: block;
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            border-bottom: 1px solid $gray;
        }
    }
}

