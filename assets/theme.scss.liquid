            &::before {
                content: "";
                position: absolute;
                left: 19px;
                top: 0;
                display: inline-block;
                width: 10px;
                height: 10px;
                border-radius: 50%;
                border: 1px solid #979797;
                //box-shadow: 0 0 0 8px white;
            }
        }
        &.active {
