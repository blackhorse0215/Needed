            -moz-transform: rotate(45deg);
            -o-transform: rotate(45deg);
        }
        @include breakpoint(extra-small) {
            &::after {
                right: -8px;
                width: 6px;
                height: 6px;
            }
        }
    }
    .banner__description {
        margin-top: 15px;
        font-size: 19px;
        line-height: 30px;
        letter-spacing: 0;
        color: $dark-primary;
        +* {
            margin-top: 30px;
        }
    }
    .desc-wrapper {
        position: relative;
        text-align: initial;
