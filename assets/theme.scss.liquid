.banner {
    height: 685px;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
    .banner__title {
        font-family: $heading-font;
        color: $dark-primary;
        letter-spacing: 0;
        font-size: 40px;
        line-height: 48px;
        strong {
            font-family: inherit;
            font-weight: normal;
            color: $primary;
        }
    }
    .banner__title-shape {
        position: relative;
        p {
            display: inline;
        }
        &::after {
            content: "";
            position: absolute;
            right: -12px;
            bottom: 7px;
            display: inline-block;
            width: 8px;
            height: 8px;
            background-color: $dark-primary;
            border-radius: 2px;
            transform: rotate(45deg);
