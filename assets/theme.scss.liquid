                        &:after {
                            right: initial;
                            width: 9px;
                            left: -20px;
                            height: 2px;
                            background-color: #1f3337;
                            content: "";
                            position: absolute;
                            top: 8px;
                            display: inline-block;
                            transform: rotate(-45deg);
