    .mobile-menu__account-item {
        position: relative;
        margin-top: 13px;
        padding-top: 26px;
        &::before {
            content: "";
            position: absolute;
            top: 0;
            left: 50%;
            display: block;
            width: 320px;
            border-top: 1px solid $gray;
            transform: translateX(-50%);
        }
    }
    .mobile-menu__link {
        position: relative;
        font-size: 20px;
        letter-spacing: 0;
        color: $dark-primary;
        text-decoration: none;
        &.mobile-menu__link--opened {
            .mobile-menu__chevron {
                transform: translateY(-50%) rotate(180deg);
            }
        }
        &.mobile-menu__link--rounded {
            display: inline-block;
            border-radius: 17px;
            border: 1px solid $primary;
            padding: 5px 15px;
            color: $primary;
        }
        &.mobile-menu__link--active {
            color: $primary;
        }
    }
    .mobile-menu__icon {
        display: none;
        //position: absolute;
        //left: -30px;
        //top: 50%;
        //transform: translateY(-50%);
        //display: inline-block;
        //width: 22px;
        //height: 22px;
        //background-repeat: no-repeat;
        //background-position: 50% 50%;
        //background-size: contain;
    }
    .mobile-menu__chevron {
        position: absolute;
        top: 50%;
        right: -30px;
        width: 13px;
        margin-left: 15px;
        transform: translateY(-50%);
        transition: .3s all;
    }
    // dropdopwn
    .mobile-menu__dropdown {
        display: none;
        width: 320px;
    }
}

@keyframes slideUpMenu {
    0% {
        transform: translateY(15px);
    }
    100% {
        transform: translateY(0);
    }
}

