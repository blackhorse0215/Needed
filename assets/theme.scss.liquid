                    }
                    &:hover {
                        .collaboration__circle-label-title {
                            left: auto;
                            right: 0;
                            top: 30px;
                        }
                    }
                    &.active {
                        .collaboration__circle-label-title {
                            left: auto;
                            right: 0;
                            top: 30px;
                        }
                    }
                }
            }
        }
        &:nth-child(3) {
            left: 50%;
            bottom: 0;
            transform: translateX(-50%);
            .collaboration__circle-label {
                left: 40px;
                bottom: 67px;
            }
        }
        &:nth-child(4) {
            position: static;
            width: 0;
            height: 0;
            //left: 50%;
            //bottom: 0;
            //transform: translateX(-50%);
            .collaboration__circle-label {
                left: 50%;
                top: 44%;
                z-index: 5;
                transform: translate(-50%, -50%);
            }
        }
        .collaboration__circle-label {
            position: absolute;
            //left: 40px;
            //bottom: calc(50% - 40px);
            width: 20px;
            height: 20px;
            border-radius: 50%;
            background-color: rgba(33, 54, 55, 0.38);
            cursor: pointer;
            transition-duration: .3s;
            transition-property: box-shadow;
            &:hover {
                .collaboration__circle-label-title {
                    left: calc(100% + 10px);
                    opacity: 1;
                    visibility: visible;
                }
            }
            &.active {
                background-color: $dark-primary;
                box-shadow: 0 0 0 4px rgba(33, 54, 55, 0.38);
                .collaboration__circle-label-title {
                    left: calc(100% + 10px);
                    opacity: 1;
                    visibility: visible;
                }
            }
        }
        .collaboration__circle-label-title {
            position: absolute;
            top: 2px;
            left: 0;
            opacity: 0;
            visibility: hidden;
            white-space: nowrap;
            font-size: 15px;
            transition: .3s all;
        }
        &.collaboration__circle--active {
            background-color: rgba(159, 161, 101, 0.6);
            z-index: 3;
        }
        @include breakpoint(medium) {
            width: 196px;
            height: 190px;
            .collaboration__circle-label {
                width: 13px;
                height: 13px;
                //left: 20px;
            }
            .collaboration__circle-label-title {
                display: none;
                top: 0;
            }
            &:nth-child(1) {
                .collaboration__circle-label {
                    //bottom: calc(50% + 20px);
                    bottom: 65px;
                }
            }
            &:nth-child(2) {
                .collaboration__circle-label {
                    //bottom: 55px;
                    bottom: 65px;
                }
            }
            &:nth-child(3) {
                .collaboration__circle-label {
                    //bottom: 55px;
                    bottom: 45px;
                }
            }
            &:nth-child(4) {
                .collaboration__circle-label {}
            }
        }
    }
    // Circles end
    .collaboration__image {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        object-fit: contain;
        object-position: 50% 50%;
        opacity: 0;
        transition: .5s all;
        &.collaboration__image--active {
            opacity: 1;
        }
    }
    .collaboration__content {
        position: relative;
        width: 50%;
        color: $text-light;
        @include breakpoint(medium) {
            width: 100%;
            margin-top: 30px;
        }
    }
    .collaboration__title {
        margin-bottom: 40px;
        &.collaboration__title--mobile {
            display: none;
        }
        @include breakpoint(medium) {
            text-align: center;
            &.collaboration__title--mobile {
                display: block;
                margin-bottom: 28px;
            }
            &.collaboration__title--desktop {
                display: none;
            }
        }
    }
    .collaboration__steps {
        position: relative;
    }
    .collaboration__step {
        position: absolute;
        left: 0;
        top: 0;
        opacity: 0;
        transition: .5s all;
        &.collaboration__step--active {
            position: relative;
            opacity: 1;
        }
    }
    // Styles for animation elements end
    .collaboration__title {
        margin-bottom: 30px;
        letter-spacing: 1.4px;
