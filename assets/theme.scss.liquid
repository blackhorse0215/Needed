// Practitioner
.practitioner-form {
    .g-form-container {
        .layout-wrap {
            padding: 0 10px;
        }
        .saveForm {
            margin-top: 0;
            margin-bottom: 0;
            text-align: center;
            .g-btn {
                display: inline-block;
                min-width: 470px;
                min-height: 57px;
                padding: 16px 25px;
                border: none;
                border-radius: 28.5px;
                box-shadow: none;
                color: #fff;
                background-color: $dark-primary;
                font-family: $base-font-bold;
                font-weight: normal;
                font-size: 16px;
                line-height: 1;
                text-align: center;
                text-decoration: none;
                transition-property: color, background-color;
                transition-duration: .3s;
                &:hover,
                &:focus {
                    background-color: $text-light;
                }

                @media (max-width: 500px) {
                    min-width: 100%;
                    max-width: 400px;
                }
            }
        }
    }
    .g-form-container .g-form-control .g-form-group {
        &.required {
            label {
                &::after {
                    content: "*";
                }
            }
        }
        &.select {
            height: auto;
            border: none;
            box-shadow: none;
            background-image: none;
        }
        position: relative;
        display: flex;
        flex-direction: column;
        flex-wrap: nowrap;
        justify-content: flex-start;
        align-items: flex-start;
        margin-bottom: 40px;
        padding: 0;
        box-shadow: none;
        label {
            order: 0;
            font-family: $base-font;
            font-size: 16px;
            letter-spacing: 0.3px;
            line-height: 28px;
            color: $text-light;
            transition: color 250ms;
            margin: 0;
            min-height: auto;
            font-weight: normal;
            &.focused {
                color: $primary;
            }
            &.error {
                //color: red;
                color: #87893e;
                left: 0;
            }
        }
        *:not(label) {
            order: 1;
        }
        input {
            width: 100%;
            height: auto;
            // border: none;
            color: $text-light;
            font-family: $base-font;
            font-size: 18px;
            line-height: 28px;
            letter-spacing: 0.34px;
            // box-shadow: inset 0 -1px 0 0 #2f2f2f;
            border-radius: 29.5px;
            background-color: #F6F4F2;
            height: 56px;
            padding: 10px 20px;
            border-color: #BCB9B5;
            -webkit-appearance: none;
            -moz-appearance: none;
            appearance: none;
            transition: box-shadow 250ms;
            &.input--error {
                box-shadow: inset 0 -1px 0 0 red;
            }
            &::-ms-clear {
                display: none;
            }
            &:focus {
                background-color: #fff;
                border-color: #1f3337;
                &+label {
                    color: $primary;
                    &.error {
                        //color: red;
                        color: #87893e;
                    }
                }
            }
            @include placeholder() {
                color: #ccc;
            }
        }
        textarea {
            width: 100%;
            height: 150px;
            padding: 10px;
            border-color: #BCB9B5;
            font-family: $base-font;
            font-size: 16px;
            border-radius: 29.5px;
            background-color: #F6F4F2;
            box-shadow: none !important;
            resize: none;
            transition-property: border-color;
            transition-duration: .3s;
            -webkit-appearance: none;
            appearance: none;
            &:focus {
                border-color: $primary;
            }
            &:focus+label {
                color: $primary;
                &.error {
                    //color: red;
                    color: #87893e;
                }
            }
            @include placeholder() {
                color: #ccc;
            }
        }
        select {
            width: 100%;
            height: 27px;
            border: none;
            border-bottom: 1px solid $text-dark;
            background-color: transparent;
            font-weight: normal;
            border-radius: 0;
            background-image: url('icon-chevron-down.png');
            background-repeat: no-repeat;
            background-size: 12px;
            background-position: top 50% right 5px;
            -webkit-appearance: none;
            -moz-appearance: none;
            appearance: none;
            color: $text-dark;
            cursor: pointer;
            font-family: $base-font;
            font-size: 18px;
            line-height: 28px;
            letter-spacing: 0.34px;
            padding: 0;
            border-bottom: none;
            box-shadow: inset 0 -1px 0 0 #2f2f2f;
            transition: box-shadow 250ms;
            &+label {
                transition: color 250ms;
            }
            &.select--error {
                box-shadow: inset 0 -1px 0 0 red;
            }
            &:focus {
                box-shadow: inset 0 -2px 0 0 $primary;
                &+label {
                    color: $primary;
                }
            }
            &[value=""] {
                color: #ccc;
            }
            option {
                color: $text-light;
                &[value=""] {
                    color: #ccc;
                }
            }
        }
        @include breakpoint(small) {
            [class*="col"]+[class*="col"] {
                margin-top: 20px;
            }
        }
        .form__addition {
            position: absolute;
            right: 0;
            top: 103%;
            padding: 10px;
            font-size: 14px;
            line-height: 0.3px;
            text-align: right;
        }
        .row+.row {
            margin-top: 30px;
        }
    }
}

