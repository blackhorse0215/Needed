// Blog
.blog-slider {
    margin-bottom: 40px;
    background-color: $light-gray-6;
    // slide
    .blog-slider__slide {
        display: flex;
        align-items: center;
        height: 685px;
        @include breakpoint(small) {
            flex-direction: column-reverse;
            height: auto;
        }
    }
    .blog-slider__content {
        width: 52%;
        padding-right: 115px;
        padding-left: 60px;
        padding-top: 80px; // reduce the header height
        @include breakpoint(medium) {
            padding-right: 75px;
        }
        @include breakpoint(small) {
            width: 100%;
            padding-top: 45px;
            padding-right: 30px;
            padding-bottom: 75px;
            padding-left: 30px;
        }
    }
    .blog-slider__information {
        max-width: 457px;
        margin-left: auto;
        opacity: 0;
        transform: translateY(-15px);
        transition-property: opacity, transform;
        transition-duration: .5s;
        transition-delay: .5s;
        @include breakpoint(small) {
            max-width: 100%;
        }
    }
    .blog-slider__title {
        display: flex;
        align-items: center;
        margin-bottom: 10px;
        font-size: 28px;
        font-family: $heading-font-extra-bold;
        font-weight: normal;
        letter-spacing: 1.1px;
        line-height: 1.2;
        color: $dark-primary;
        @include breakpoint(medium) {
            font-size: 22px;
        }
    }
    .blog-slider__title-icon {
        width: 33px;
        height: 33px;
        background-repeat: no-repeat;
        background-position: 50% 50%;
        background-size: contain;
        flex-shrink: 0;
        margin-right: 10px;
        &[data-icon=""] {
            display: none;
        }
    }
    .blog-slider__description {
        line-height: 1.56;
        letter-spacing: 0.4px;
        color: $text-dark;
        +* {
            margin-top: 30px;
        }
    }
    .blog-slider__image {
        width: 48%;
        height: 100%;
        align-self: stretch;
        position: relative;
        overflow: hidden;
        img {
            position: absolute;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            object-fit: cover;
            object-position: 50% 50%;
        }
        @include breakpoint(small) {
            width: 100%;
            height: 350px;
        }
    }
    .slick-active {
        .blog-slider__information {
            opacity: 1;
            transform: translateY(0);
        }
    }
    .slick-dots {
        position: absolute;
        top: 50%;
        left: calc(52% - 497px - 115px); // the 52%-content width + (457px-width-text + 48 indent from dots) - 115px- padding-right of content blog
        margin: 0;
        flex-direction: column;
        transform: translateY(-50%);
        li {
            margin: 3px;
        }
        @media (max-width: 1300px) {
            left: 25px;
        }
        @include breakpoint(small) {
            flex-direction: row;
            top: auto;
            left: 50%;
            bottom: 30px;
            transform: translateY(0) translateX(-50%);
        }
    }
}

.filters {
    display: flex;
    flex-wrap: wrap;
    margin-top: 75px;
    margin-bottom: 60px;
    .filters__label {
        display: inline-flex;
        margin-right: 5px;
        padding-top: 5px;
