// Nature first
.nature-sliced {
    position: relative;
    overflow: hidden;
    height: 200vh; // real value would be set in JS with script
    @include breakpoint(medium) {
        height: auto !important;
    }
    .nature-sliced__wrapper {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100vh;
        z-index: 10;
        display: flex;
        padding-top: 120px;
        &.sticky {
            // Styles for moment, when wrapper start scrolled in nature-sliced
            position: fixed;
            //@media (min-width: 1200px) {
            //	.nature-sliced__content {
            //		transform: translate3d(0, 27vh, 0);
            //	}
            //}
        }
        &.bottom-position {
            // Styles for moment, when wrapper scrolled to the bottom of the nature-sliced
            top: auto;
            bottom: 0;
            //@media (min-width: 1200px) {
            //	.nature-sliced__content {
            //		transform: translate3d(0, 27vh, 0);
            //	}
            //}
        }
        @include breakpoint(medium) {
            position: static;
            height: auto;
            padding-top: 0;
            &.sticky {
                position: static;
            }
        }
    }
    .nature-sliced__container {
        display: flex;
        flex-wrap: wrap;
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;
        padding: 0 10px;
    }
    .nature-sliced__photo {
        position: absolute;
        right: 0;
        top: auto;
        bottom: 0;
        width: 95vw;
        height: 90vh;
        @include breakpoint(medium) {
            display: none;
        }
    }
    .nature-sliced__image {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
        object-position: 50% 50%;
        opacity: 0;
        transition: .5s all;
        -webkit-clip-path: polygon(100% 0, 0 100%, 100% 100%);
        clip-path: polygon(100% 0, 0 100%, 100% 100%);
        &.nature-sliced__image--active {
            opacity: 1;
        }
    }
    .nature-sliced__content {
        position: relative;
        width: 50%;
        //padding-top: 120px;
        z-index: 5;
        align-self: flex-start;
        color: $text-light;
        //transform: translate3d(0, 0, 0);
        //transition-duration: .7s;
        //transition-timing-function: ease;
        //transition-property: transform;
        @include breakpoint(medium) {
            padding-left: 30px;
            width: 55%;
            transform: none;
        }
        @include breakpoint(medium) {
            width: 100%;
            padding-top: 40px;
            padding-left: 10px;
            padding-right: 10px;
            padding-bottom: 40px;
        }
    }
    .nature-sliced__title {
        margin-bottom: 8px;
        font-size: 24px;
        letter-spacing: 1.4px;
        color: $dark-primary;
        @include breakpoint(extra-small) {
            font-size: 20px;
        }
    }
    .nature-sliced__subtitle {
        font-size: 18px;
        line-height: 1.11;
        letter-spacing: 0.4px;
        color: $text-dark;
    }
    .nature-sliced__steps {
        position: relative;
        margin-top: 32px;
    }
    .nature-sliced__step {
        position: absolute;
        left: 0;
        top: 0;
        opacity: 0;
        padding-top: 10px;
        padding-left: 60px;
        transition: .5s all;
        &.nature-sliced__step--active {
            position: relative;
            opacity: 1;
        }
        @include breakpoint(medium) {
            position: relative;
            opacity: 1;
            padding-top: 7px;
            padding-left: 45px;
            +.nature-sliced__step {
                margin-top: 40px;
            }
        }
    }
    .nature-sliced__step-icon {
        position: absolute;
        left: 0;
        top: 0;
        width: 44px;
        @include breakpoint(medium) {
            width: 36px;
        }
    }
    .nature-sliced__step-title {
        margin-bottom: 15px;
        font-family: $base-font-bold;
        font-size: 24px;
        line-height: 1;
        letter-spacing: 0.5px;
        color: $text-dark;
        strong {
            font-weight: normal;
            color: $primary;
        }
        @include breakpoint(medium) {
            font-size: 20px;
            line-height: 1.2;
        }
    }
    .nature-sliced__text {
        line-height: 1.56;
        letter-spacing: 0.3px;
        color: $text-light;
        p+p {
            margin-top: 15px;
        }
    }
}

