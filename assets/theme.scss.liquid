.popup {
    position: fixed;
    left: 0;
    top: 0;
    width: 100vw;
    height: 100vh;
    z-index: 200;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow-y: auto;
    padding: 15px 15px 33px;
    visibility: hidden;
    opacity: 0;
    transition: opacity 300ms, visibility 300ms;
    @include breakpoint(small) {
        padding-bottom: 90px;
    }
    // IMPORTANT IOS FIX
    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0,
    0,
    0);
    background-color: rgba(0,
    0,
    0,
    0.35);
    &.popup--opened {
        visibility: visible;
        opacity: 1;
        z-index: 250;
    }
    .popup__container {
        z-index: 202;
        position: relative;
        margin: auto;
        .popup-img-wrapper {
            width: 450px;
            height: 600px;
            background-size: 450px 600px;
            background-position: center;
            background-repeat: no-repeat;
            img {
                max-height: 80vh;
                visibility: hidden;
            }
            @include breakpoint(small-tablet-portrait) {
                width: 320px;
                height: 450px;
                background-size: 320px 450px;
            }
        }
    }
    .popup__close-wrapper {
        position: absolute;
        right: 12px;
        top: 12px;
        background-color: transparent;
        padding: 0;
        cursor: pointer;
    }
    .popup__close {
        position: relative;
        width: 15px;
        height: 15px;
        display: block;
        cursor: pointer;
        &:before,
        &:after {
            content: '';
            position: absolute;
            left: 50%;
            top: 50%;
            width: 100%;
            height: 2px;
            background-color: #cfd0b2;
            transition: background-color 250ms;
        }
        &:after {
            transform: translate(-50%, -50%) rotate(45deg);
            -ms-transform: translate(-50%, -50%) rotate(45deg);
            -webkit-transform: translate(-50%, -50%) rotate(45deg);
            -moz-transform: translate(-50%, -50%) rotate(45deg);
            -o-transform: translate(-50%, -50%) rotate(45deg);
        }
        &:before {
            transform: translate(-50%, -50%) rotate(-45deg);
            -ms-transform: translate(-50%, -50%) rotate(-45deg);
            -webkit-transform: translate(-50%, -50%) rotate(-45deg);
            -moz-transform: translate(-50%, -50%) rotate(-45deg);
            -o-transform: translate(-50%, -50%) rotate(-45deg);
        }
        &:hover {
            &:after,
            &:before {
                background-color: $text-dark;
            }
        }
        @include breakpoint(small) {
            width: 24px;
            height: 24px;
        }
    }
}

.accordion {
    .accordion__pane {
        border-bottom: 1px solid #d3d7d7;
        padding: 10px;
        &:first-child {
            border-top: 1px solid #d3d7d7;
        }
        &:last-child {
            border-bottom: 0;
        }
    }
    .accordion__title {
        color: $text-light;
        display: flex;
        justify-content: space-between;
        align-items: center;
        line-height: 27px;
        span {
            cursor: pointer;
        }
    }
    .accordion__state {
        position: relative;
        width: 11px;
        height: 11px;
        &:after,
        &:before {
            content: '';
            display: block;
            position: absolute;
            top: 4.5px;
            left: 0;
            width: 100%;
            height: 2px;
            background: $text-light;
            transition: transform 300ms, width 300ms;
        }
        // Vertical line
        &:after {
            transform-origin: 50% 50%;
            transform: rotate(90deg);
            -ms-transform: rotate(90deg);
            -webkit-transform: rotate(90deg);
            -moz-transform: rotate(90deg);
            -o-transform: rotate(90deg);
        }
        &.accordion__state--active {
            &:after {
                transform: rotate(0deg);
                -ms-transform: rotate(0deg);
                -webkit-transform: rotate(0deg);
                -moz-transform: rotate(0deg);
                -o-transform: rotate(0deg);
                // width: 0;
            }
        }
        @include breakpoint(small) {
            width: 20px;
            height: 20px;
            &:after,
            &:before {
                top: 9px;
                left: 5px;
                width: 11px;
            }
        }
    }
    .accordion__content {
        margin-top: 15px;
    }
}

