// Tailored
.sliced-scroll {
    padding-top: 30px;
    padding-bottom: 80px;
    overflow: hidden;
    @include breakpoint(extra-small) {
        padding-bottom: 20px;
    }
    .sliced-scroll__row {
        display: flex;
        flex-wrap: wrap;
    }
    .sliced-scroll__image {
        width: 60%;
        @include breakpoint(medium) {
            display: none;
        }
    }
    .sliced-scroll__content {
        position: relative;
        width: 40%;
        padding-top: 60px;
        @include breakpoint(medium) {
            width: 100%;
            padding-top: 0;
        }
    }
    .sliced-scroll__image-wrapper {
        position: relative;
        width: 112%;
        padding-top: 72%;
    }
    .sliced-scroll__image-sliced {
        position: absolute;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        object-fit: cover;
        object-position: 50% 50%;
        max-width: none;
        -webkit-clip-path: polygon(0 0, 100% 0, 57% 100%, 0% 100%);
        clip-path: polygon(0 0, 100% 0, 57% 100%, 0% 100%);
        opacity: 0;
        transition-property: opacity;
        transition-duration: .3s;
        &.active {
            opacity: 1;
        }
    }
    .sliced-scroll__title {
        margin-bottom: 10px;
        font-size: 24px;
        font-family: $heading-font-extra-bold;
        font-weight: normal;
        line-height: normal;
        letter-spacing: 1.4px;
        color: $dark-primary;
