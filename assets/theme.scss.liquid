        //&::before,
        //&::after {
        //    content: "";
        //    position: absolute;
        //    left: 50%;
        //    width: 10px;
        //    height: 6px;
        //    background-color: #fff;
        //    transform: translateX(-50%);
        //}
        //
        //&::before {
        //    top: -6px;
        //}
        //
        //&::after {
        //    bottom: -6px;
        //}
        img {
            vertical-align: middle;
        }
    }
    .sliced-scroll__item-tag {
        display: block;
        margin-bottom: 8px;
        font-family: $base-font-bold;
        font-weight: normal;
        font-size: 15px;
        line-height: 1.47;
        letter-spacing: 0.3px;
        color: $dark-primary;
    }
    .sliced-scroll__item-title {
        margin-bottom: 5px;
        font-family: $base-font-bold;
        font-weight: normal;
        line-height: 1.22;
        letter-spacing: 0.4px;
        color: $dark-primary;
